    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e">
