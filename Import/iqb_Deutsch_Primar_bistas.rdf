        <skos:notation>1.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">verfügen über einen angemessenen Wortschatz,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/49d9cfd2-ae2e-4d06-9ca9-90baed2a550f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/49d9cfd2-ae2e-4d06-9ca9-90baed2a550f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9df1540c-4ffb-472c-9c70-77296d7a1014"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">wenden in vertrauten und überschaubaren Situationen erste kognitive und metakognitive Strategien vor, während und nach dem Zuhören an, z. B. Blickkontakt herstellen, Fragen stellen, Medium mehrmals hören, Hörerwartung aufbauen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/490ab89e-5778-4c1b-afa6-a8b97f763b96 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/490ab89e-5778-4c1b-afa6-a8b97f763b96">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9fabf521-1dc9-48b1-8a04-8e3958e71f17"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.5.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">Ausdrücken: Sich selbst, Erlebtes und Erdachtes, Gedanken und Gefühle (auch auf Basis von Schreibimpulsen wie Texten, Bildern oder Musik), z. B. in den Textsorten: Erlebniserzählung, Fantasieerzählung, Selbstportrait, Gedicht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/4941b5be-de51-4141-b60d-e29d59a71aa5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/4941b5be-de51-4141-b60d-e29d59a71aa5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d212d31c-fbeb-4484-b8e6-7d52c1bd840c"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen ausgewählte auditive und audiovisuelle Medien für persönliche oder schulische Zwecke: z. B. Hörspiele, Lesungen, Hörbücher, Podcasts, Wissenssendungen, Erklärvideos,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/497b15ba-f68b-4019-ae54-fdd4335b9536 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/497b15ba-f68b-4019-ae54-fdd4335b9536">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/e571d29a-a7fa-48cd-90ae-c0fb02ba9a74"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können über einen kurzen, begrenzten Zeitraum (vorbereitet und durch Medien gestützt) sprechen und eigene kommunikative Ziele verfolgen (Ausdrucks-, Darstellungs- und Appellfunktion).</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/a9a97800-2d1f-49bb-bba7-3ca21e9b683f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/f5fe0268-31ab-4e2f-ba26-1d788ea605dd"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/fa97ed3e-77f5-48fc-89ba-5b44974f9594"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/6fe65df6-9ba8-4092-8486-2efd5e5d7a83"/>
        <skos:notation>1.4</skos:notation>
        <skos:prefLabel xml:lang="de">Vor anderen sprechen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5a2f94d3-e839-4bc1-9420-49e2e62f7a49 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5a2f94d3-e839-4bc1-9420-49e2e62f7a49">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/44f5059c-1142-40a0-b968-8a004e49cb62"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bbbe10dd-6064-4fe9-a1d1-22eb5ca2ea79"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/c24c391e-edc8-41e3-aaab-11cabae8f9bd"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/e1fba8e4-8f03-4924-8f32-ab7fbfe092bc"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/e3a1acfa-a1a0-443d-8d62-51dfde6c8c84"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/fb3843ee-3443-4739-ba5b-e3224341f929"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/8bf415d2-f4f8-4f64-890b-2391b0fbbfc9"/>
        <skos:notation>5.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Wörter und Sätze</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5b910c5c-dc4c-4e64-9b56-8d5fbbf30d04 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5b910c5c-dc4c-4e64-9b56-8d5fbbf30d04">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">beachten (gemeinsam entwickelte) Gesprächsregeln; achten auf eine wertschätzende Gesprächsatmosphäre,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5be60709-f94f-4bcc-b557-9c370093037f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5be60709-f94f-4bcc-b557-9c370093037f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">sammeln Ideen, ordnen Informationen und fertigen eine Gliederung an, z. B. numerische Gliederung, Cluster, Mindmap,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5cdbc872-887b-48a0-92ad-2e1da0c1d54a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5cdbc872-887b-48a0-92ad-2e1da0c1d54a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/39c96913-a65b-406c-a93c-0c670d32e944"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">verfügen bei häufig vorkommenden Wörtern über eine weitgehend sichere Worterkennung und korrigieren sich ggf. selbst,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/50f4b5b1-c93e-4780-addd-41bd8d6faed1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/50f4b5b1-c93e-4780-addd-41bd8d6faed1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.6</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Vorbereitung der Textproduktion, z. B. Anlegen von Stoffsammlungen, Mindmaps.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/519a875d-3931-4de4-971e-8fb57872e45c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/519a875d-3931-4de4-971e-8fb57872e45c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/97a08c90-2a78-4c7d-990b-c83eb664bb54"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">kennen und nutzen folgende Interpunktionszeichen: Punkt, Komma bei Aufzählungen, Fragezeichen, Ausrufezeichen, Anführungszeichen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/546a3dfd-c923-478d-9360-6cb347e6a363 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/546a3dfd-c923-478d-9360-6cb347e6a363">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9075b558-60f9-4efe-bc03-88644bb2a19e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.6.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">bekannte Adressaten</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5496e722-48b7-4833-99e4-2cc00ad9b846 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5496e722-48b7-4833-99e4-2cc00ad9b846">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/263879a9-be69-49fb-bf28-b0a48ceab1f8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen bei digitalen Texten Navigationsstrukturen (u. a. Hyperlinks) zur Gewinnung von Textinformationen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/56bb99e4-dc86-4463-968f-1241f078f2ec -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/56bb99e4-dc86-4463-968f-1241f078f2ec">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">beteiligen sich an vertrauten und überschaubaren Gesprächssituationen mit verständlichen, zuhörerbezogenen, themenbezogenen und konstruktiven Beiträgen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5631b827-57a6-4899-b26e-ff99995d4f8e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5631b827-57a6-4899-b26e-ff99995d4f8e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">sammeln erste sprachliche Mittel: Wörter und Wortfelder</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/5669c873-2fc9-4c80-a931-5b2b197898b8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/5669c873-2fc9-4c80-a931-5b2b197898b8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/33f483df-35af-4c5c-85ab-96d4d8b531b9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.5.6</skos:notation>
        <skos:prefLabel xml:lang="de">kennen grundlegende Formulierungsstrategien und wenden sie an.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6d4857c5-a64c-4d19-93de-2cf4ca4dc6cb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6d4857c5-a64c-4d19-93de-2cf4ca4dc6cb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d212d31c-fbeb-4484-b8e6-7d52c1bd840c"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">kennen verschiedene Textsorten aus dem Bereich pragmatischer Texte: informierende (z. B. Lexikonartikel), instruierende (z. B. Spielanleitungen), regulierende (z. B. Klassenregeln),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6e945077-610f-4b4d-b0b8-03035768dca8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6e945077-610f-4b4d-b0b8-03035768dca8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bd7b288d-2ce2-4123-adfb-25ea09b34960"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.1.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">erklären</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6ebb3692-bc35-43f2-b7aa-11ec082c959e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6ebb3692-bc35-43f2-b7aa-11ec082c959e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/681234ec-6302-4a14-8716-b97e68ae8c89"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.2.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen die Konventionen der gesprochenen Standardsprache für die Gestaltung ihrer Äußerungen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6fe65df6-9ba8-4092-8486-2efd5e5d7a83 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6fe65df6-9ba8-4092-8486-2efd5e5d7a83">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/497b15ba-f68b-4019-ae54-fdd4335b9536"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">leisten kurze, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Kurzreferat, Präsentation, Gedicht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6052a51b-692d-4cea-8f96-3a31d719661d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6052a51b-692d-4cea-8f96-3a31d719661d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.3.8</skos:notation>
        <skos:prefLabel xml:lang="de">belegen Deutungsaussagen mit Textstellen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/6068b5ff-153e-481a-bb08-b7e2b42e580a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/6068b5ff-153e-481a-bb08-b7e2b42e580a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/79cec164-b3db-4e6b-b699-bbe871f5312f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen unterschiedliche Informationsangebote und Recherchemöglichkeiten (u. a. Internet, Zeitschriften, Zeitungen, Hörfunk, Fernsehen, Bücherei),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/676defb1-5d8d-4e81-82bf-65aa89b963b2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/676defb1-5d8d-4e81-82bf-65aa89b963b2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.3.11</skos:notation>
        <skos:prefLabel xml:lang="de">tauschen sich mit anderen zu Text- bzw. Medienwahrnehmungen, -deutungen und -wertungen aus (u. a. literarisches Gespräch),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/681234ec-6302-4a14-8716-b97e68ae8c89 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/681234ec-6302-4a14-8716-b97e68ae8c89">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/44f5059c-1142-40a0-b968-8a004e49cb62"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/6ebb3692-bc35-43f2-b7aa-11ec082c959e"/>
        <skos:notation>5.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">Aussprache</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/7da304ef-8501-4ee2-a9dc-e1a4ba7de9da -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/7da304ef-8501-4ee2-a9dc-e1a4ba7de9da">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/84eca6b1-9008-4818-aca1-58f97e60e438"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben Texte in leserlicher Handschrift und mithilfe digitaler Schreibwerkzeuge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/704b31bf-78ab-48a5-b6a6-029e607098c2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/704b31bf-78ab-48a5-b6a6-029e607098c2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.5</skos:notation>
        <skos:prefLabel xml:lang="de">führen einzelne Planungsschritte (Inhalt, Aufbau) kooperativ durch,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/73c3941e-992d-4999-8c4c-894a86f54c4f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/73c3941e-992d-4999-8c4c-894a86f54c4f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9075b558-60f9-4efe-bc03-88644bb2a19e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.6.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">treffende Wortwahl, Rechtschreibung, Gestaltung,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/74c625f1-a6b7-454e-951e-9208ede416de -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/74c625f1-a6b7-454e-951e-9208ede416de">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/8255bbf1-fbe6-4dbb-a02e-ed6f58318937"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">bereiten das Lesen vor (z. B. Leseziel formulieren, Vorwissen aktivieren, Erwartungen an den Text formulieren, nach Möglichkeit die Textsorte klären),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/77fca718-1864-4c1c-bc39-f1b5f3dd12e1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/77fca718-1864-4c1c-bc39-f1b5f3dd12e1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/42370466-9a27-4aa7-94c6-0e94a5db09f5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.1.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">sprechen über Verstehens- und Verständigungsprobleme,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/79cec164-b3db-4e6b-b699-bbe871f5312f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/79cec164-b3db-4e6b-b699-bbe871f5312f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/0446c36a-e062-4576-b4c7-e1e82f8522bc"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler nutzen das Medienangebot zur Entwicklung und Pflege eigener Interessen, zur Erweiterung ihrer Wissensbasis und als Anlass zum Austausch mit anderen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bded9def-94d0-41cf-9c93-bf0793115f95"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/e0d25dbe-d95d-4bb2-951c-8f3dad13fe7d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/279c6fd3-6ab8-4050-ab3a-5eb9c84e4df3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/6068b5ff-153e-481a-bb08-b7e2b42e580a"/>
        <skos:notation>4.2</skos:notation>
        <skos:prefLabel xml:lang="de">Sich im Medienangebot orientieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/8aac6475-0176-48fb-901f-a32f0dc184e1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/8aac6475-0176-48fb-901f-a32f0dc184e1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d212d31c-fbeb-4484-b8e6-7d52c1bd840c"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden erzählende, lyrische und szenische Texte (einfache Beispiele),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/8b086786-0da3-465c-ba76-152b7d42ea87 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/8b086786-0da3-465c-ba76-152b7d42ea87">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/97a08c90-2a78-4c7d-990b-c83eb664bb54"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben häufig vorkommende Fremdwörter richtig (z. B. Melone, Vulkan, Maschine, Kabine)</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/8bf415d2-f4f8-4f64-890b-2391b0fbbfc9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/8bf415d2-f4f8-4f64-890b-2391b0fbbfc9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/5a2f94d3-e839-4bc1-9420-49e2e62f7a49"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.2.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen grundlegende Satzstrukturen (insbesondere Prädikat, Satzglieder) und die Stellung des finiten Verbs im Satz,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/80303b1a-1b6b-4053-822e-4b7586ef322e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/80303b1a-1b6b-4053-822e-4b7586ef322e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/97a08c90-2a78-4c7d-990b-c83eb664bb54"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.2.7</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Rechtschreibhilfen (Wörterbuch und digitale Rechtschreibhilfe),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/82b82b8b-c72b-4366-8fa5-d0d73edd4750 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/82b82b8b-c72b-4366-8fa5-d0d73edd4750">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d212d31c-fbeb-4484-b8e6-7d52c1bd840c"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.1.6</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen typische Formen digitaler Textstrukturierung (u. a. Links/Hyperlinks, Text-Bild-Kombinationen) beim Lesen sowie beim Recherchieren.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/8255bbf1-fbe6-4dbb-a02e-ed6f58318937 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/8255bbf1-fbe6-4dbb-a02e-ed6f58318937">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1ac5b2ff-4ac3-40ff-a3d4-b45ead55c80b"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler kennen erste grundlegende kognitive und metakognitive Lesestrategien zum Einsatz vor dem Lesen, während des Lesens und nach dem Lesen. Sie können diese bei gut strukturierten Texten anwenden.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/172a1261-0cba-48ea-b939-e589b92ac778"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/74c625f1-a6b7-454e-951e-9208ede416de"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9c710c38-6265-4050-9072-ea9a8e1f3486"/>
        <skos:notation>3.4</skos:notation>
        <skos:prefLabel xml:lang="de">Lesefähigkeiten: Über Strategien zum Leseverstehen verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/839dec82-bcaa-4a4e-bdf2-18aec42d707c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/839dec82-bcaa-4a4e-bdf2-18aec42d707c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/263879a9-be69-49fb-bf28-b0a48ceab1f8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">stellen bei multimodalen Texten einfache Bezüge zwischen Text, Bild und Ton her,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/84eca6b1-9008-4818-aca1-58f97e60e438 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/84eca6b1-9008-4818-aca1-58f97e60e438">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/8533633b-4ec8-4ab4-a7cd-86beb3f2f71e"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler schreiben kurze Sätze flüssig. Sie schreiben in einer leserlichen Handschrift. Die Schülerinnen und Schüler schreiben auch mithilfe digitaler Schreibwerkzeuge. Sie  gestalten ihre Texte zweckmäßig und übersichtlich.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d7ae7f51-e7d3-44f1-9012-d9b1833b2ebf"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/7da304ef-8501-4ee2-a9dc-e1a4ba7de9da"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/841fe177-e3b1-4764-a7e3-9942bdea5e2c"/>
        <skos:notation>2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Über Schreibfertigkeiten verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/841fe177-e3b1-4764-a7e3-9942bdea5e2c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/841fe177-e3b1-4764-a7e3-9942bdea5e2c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/84eca6b1-9008-4818-aca1-58f97e60e438"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben Buchstaben, Wörter, Wortgruppen und kurze Sätze flüssig, d. h. zügig, sicher und korrekt (automatisiert),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/8533633b-4ec8-4ab4-a7cd-86beb3f2f71e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/8533633b-4ec8-4ab4-a7cd-86beb3f2f71e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:definition xml:lang="de">Im Unterschied zum Sprechen wird beim Schreiben in handschriftlicher wie digitaler Form der Text langsamer und überlegter entwickelt, über Geschriebenes kann auch mit anderen nachgedacht und es kann entsprechend überarbeitet werden. Die Schülerinnen und Schüler verfügen über grundlegende Schreibfertigkeiten und eine weitgehend korrekte Rechtschreibung sowie über grundlegende Möglichkeiten des Schreibens als Mittel der Kommunikation. Sie schreiben kurze Sätze flüssig. Sie können Texte zur Unterstützung des Lesens in den Kernbereichen der Orthografie richtig schreiben. Orthografische Zweifelsfälle können sie mithilfe orthografischer Strategien und durch Nachschlagen klären. Sie beherrschen die Zeichensetzung im Kernbereich. Sie haben grundlegende Einsicht in die Prinzipien der Rechtschreibung gewonnen, eine individuelle Fehleraufmerksamkeit ausgebildet und gelangen durch das Anwenden von Rechtschreibstrategien und Nachschlagen zu richtigen Schreibungen. Die Schülerinnen und Schüler schreiben, um zu kommunizieren, um Informationen festzuhalten, um Gedanken und Gefühle auszudrücken und um kreativ und gestaltend mit Sprache umzugehen. Sie verfassen für konkrete und überwiegend bekannte Leserinnen und Leser adressatenorientiert einfache Texte zu bekannten Textsorten mit Ausdrucks-, Darstellungs- und Appellfunktion. Ihren Schreibprozess gestalten sie sowohl angeleitet als auch unterstützt durch kooperative Schreibumgebungen. Im Schreibprozess berücksichtigen sie bereits grundlegende Anforderungen an Texte im Hinblick auf allgemeine Texteigenschaften, auf Adressaten, Textfunktion, Schreibanlass und Textsorte. Sie nutzen Formen kooperativen Schreibens, digitale Schreibwerkzeuge und Schreibstrategien.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/a7a85cc9-23a5-4891-b894-5d9996d51ea5"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/aa527a4d-b704-49d6-ab28-45988a869714"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/33f483df-35af-4c5c-85ab-96d4d8b531b9"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/84eca6b1-9008-4818-aca1-58f97e60e438"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/97a08c90-2a78-4c7d-990b-c83eb664bb54"/>
        <skos:notation>2</skos:notation>
        <skos:prefLabel xml:lang="de">Schreiben</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/8533633b-4ec8-4ab4-a7cd-86beb3f2f71e"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können ihre Schreibumgebung (Arbeitsplatz mit Blatt oder Heft und Stift bzw. digitalen Schreibumgebungen) einrichten und einfache Planungsschritte durchführen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/fc85c3d3-b117-4271-ba65-66323ae2c5ff"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/5be60709-f94f-4bcc-b557-9c370093037f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/50f4b5b1-c93e-4780-addd-41bd8d6faed1"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/5631b827-57a6-4899-b26e-ff99995d4f8e"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/704b31bf-78ab-48a5-b6a6-029e607098c2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9c5fe1fc-b644-46e5-890b-f4b362b2197e"/>
        <skos:notation>2.4</skos:notation>
        <skos:prefLabel xml:lang="de">Texte planen und strukturieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/9b8d685e-2063-4339-accd-627dcaafae82 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/9b8d685e-2063-4339-accd-627dcaafae82">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/97a08c90-2a78-4c7d-990b-c83eb664bb54"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben Kernwörter richtig (Inhalts- und Funktionswörter),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/9c5fe1fc-b644-46e5-890b-f4b362b2197e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/9c5fe1fc-b644-46e5-890b-f4b362b2197e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.3</skos:notation>
        <skos:prefLabel xml:lang="de">stellen Überlegungen zu den Adressaten (konkret, bekannt), deren thematischem Vorwissen und ihren angenommenen Lesezielen an,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/9c710c38-6265-4050-9072-ea9a8e1f3486 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/9c710c38-6265-4050-9072-ea9a8e1f3486">
