    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/9fabf521-1dc9-48b1-8a04-8e3958e71f17 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/9fabf521-1dc9-48b1-8a04-8e3958e71f17">
