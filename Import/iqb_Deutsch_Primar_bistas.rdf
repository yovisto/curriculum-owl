        <skos:notation>2.5</skos:notation>
        <skos:prefLabel xml:lang="de">Texte formulieren </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/35ea8948-d8fd-47bd-98de-e5b377aeb94d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/35ea8948-d8fd-47bd-98de-e5b377aeb94d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
