        <skos:notation>2.2.6</skos:notation>
        <skos:prefLabel xml:lang="de">verfügen über Fehleraufmerksamkeit und nutzen Strategien zur Klärung orthografischer Fragen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/26cafa9b-cebd-482f-8e3f-b06840fee514 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/26cafa9b-cebd-482f-8e3f-b06840fee514">
