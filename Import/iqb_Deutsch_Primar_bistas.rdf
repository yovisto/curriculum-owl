        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/9d9d87aa-7463-4b35-a0a7-3f67452eb63b"/>
        <skos:notation>2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Orthografisch schreiben</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/98f51c23-54b7-4799-8c04-cf0582957eb8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/98f51c23-54b7-4799-8c04-cf0582957eb8">
