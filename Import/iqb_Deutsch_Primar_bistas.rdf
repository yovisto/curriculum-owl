    <!-- https://w3id.org/iqb/bista22dep/bistas/279c6fd3-6ab8-4050-ab3a-5eb9c84e4df3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/279c6fd3-6ab8-4050-ab3a-5eb9c84e4df3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/79cec164-b3db-4e6b-b699-bbe871f5312f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">wählen Bücher und Texte unterschiedlicher medialer Form auf der Basis von Interessen und Vorlieben sowie aufgabenbezogen aus; sie begründen und bewerten ihre Auswahl,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/2856838e-8dff-45cf-a138-f55c85de5eb6 -->

