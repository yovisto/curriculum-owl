    <!-- https://w3id.org/iqb/bista22dep/bistas/f0e97f8c-0297-4e72-9a55-36b63586cf42 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f0e97f8c-0297-4e72-9a55-36b63586cf42">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/39c96913-a65b-406c-a93c-0c670d32e944"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">erlesen unvertraute Wörter anhand größerer Einheiten wie Silbe oder Morphem,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f1666f43-6596-4ea3-ad10-0bf5d5e12467 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f1666f43-6596-4ea3-ad10-0bf5d5e12467">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/263879a9-be69-49fb-bf28-b0a48ceab1f8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>3.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">verknüpfen Textinformationen, ziehen auch unter Nutzung ihres Vorwissens Schlussfolgerungen und konstruieren ein Gesamtverständnis.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f17f7d0a-8c27-4162-b5b4-4c17aa1e299c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f17f7d0a-8c27-4162-b5b4-4c17aa1e299c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/33f483df-35af-4c5c-85ab-96d4d8b531b9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.5.5</skos:notation>
        <skos:prefLabel xml:lang="de">kennen ausgewählte Möglichkeiten digitaler Textgestaltung und nutzen sie angeleitet,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f2d51d99-29e5-479d-a994-0c573cba2f1e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f2d51d99-29e5-479d-a994-0c573cba2f1e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/a7a85cc9-23a5-4891-b894-5d9996d51ea5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.6.3</skos:notation>
        <skos:prefLabel xml:lang="de">holen Text-Feedback ein und formulieren selbst auch Feedback zu Texten anderer,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f5fe0268-31ab-4e2f-ba26-1d788ea605dd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f5fe0268-31ab-4e2f-ba26-1d788ea605dd">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/497b15ba-f68b-4019-ae54-fdd4335b9536"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.4.3</skos:notation>
        <skos:prefLabel xml:lang="de">planen und gestalten ausgewählte Formen des Präsentierens, z. B. Standbild, Kurzreferat, </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f6e1284e-6732-4b10-9986-740022a53cd7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f6e1284e-6732-4b10-9986-740022a53cd7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1dc8aa26-934a-4180-9bdb-1db137ee7160"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bf13418f-f080-490f-aa0b-ac3cef8023e4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/0e718237-2e96-444e-80da-d0e4ed3cb658"/>
        <skos:notation>5.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">Ausprägungen und Bedingungen sprachlicher Variation und Vielfalt</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f753a05f-ce49-4b4d-9edb-4c54fad469f1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f753a05f-ce49-4b4d-9edb-4c54fad469f1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/addda665-85ff-4ba1-95ef-8b76fe872e46"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.1.6</skos:notation>
        <skos:prefLabel xml:lang="de">berücksichtigen ausgewählte Wirkungen ihrer Redeweise situations- und adressatengerecht: Lautstärke, Betonung, Sprechtempo, Pausen, Körpersprache (Gestik/Mimik), Körperhaltung,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/f91ca04a-9c28-406b-9644-385c4318f173 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/f91ca04a-9c28-406b-9644-385c4318f173">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/b297513c-86aa-40de-a0ec-72a5893bb788"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.1.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen und gestalten die Rollen von Sprecher/Schreiber – Hörer/Leser.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/fa97ed3e-77f5-48fc-89ba-5b44974f9594 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/fa97ed3e-77f5-48fc-89ba-5b44974f9594">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/497b15ba-f68b-4019-ae54-fdd4335b9536"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen ausgewählte Werkzeuge für das Sprechen vor anderen, z. B. Tafel, Plakat, Moderationskarten, Präsentationssoftware, Whiteboard,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/fb3843ee-3443-4739-ba5b-e3224341f929 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/fb3843ee-3443-4739-ba5b-e3224341f929">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/5a2f94d3-e839-4bc1-9420-49e2e62f7a49"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.2.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen ausgewählte grammatische Operationen und grundlegende Fachbegriffe für die Untersuchung sprachlicher Struktureinheiten (s. Übersicht: Grundlegende sprachliche Strukturen und Begriffe),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/fc85c3d3-b117-4271-ba65-66323ae2c5ff -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/fc85c3d3-b117-4271-ba65-66323ae2c5ff">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/889fe4fd-ed97-4cf1-8a31-7ffb6ea9d899"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.4.4</skos:notation>
        <skos:prefLabel xml:lang="de">klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0c06f738-83c5-4560-8d2e-602dedec8d47 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0c06f738-83c5-4560-8d2e-602dedec8d47">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">beschreiben die eigene Lese- und Medienerfahrung (z. B. mithilfe eines Lese-/Medientagebuchs),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0e718237-2e96-444e-80da-d0e4ed3cb658 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0e718237-2e96-444e-80da-d0e4ed3cb658">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/f6e1284e-6732-4b10-9986-740022a53cd7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen an ausgewählten Beispielen Gemeinsamkeiten und Unterschiede verschiedener Sprachen (u. a. Herkunftssprachen, Fremdsprachen).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0f5eb9f4-7fae-44a4-8b0f-87e51dac8820 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0f5eb9f4-7fae-44a4-8b0f-87e51dac8820">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/a7a85cc9-23a5-4891-b894-5d9996d51ea5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.6.4</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen bei der Textüberarbeitung angeleitet digitale Schreibwerkzeuge, z. B. Einfügen und Umstellen von Wörtern, Wortgruppen und Sätzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/00d34d00-75b0-4498-82f5-db9da90ad8e7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/00d34d00-75b0-4498-82f5-db9da90ad8e7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d8fa143b-c17a-4ddd-9fce-de576e8dc837"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">erfassen dem Lernstand entsprechende Hörtexte, Gesprächsphasen oder Gespräche (z. B. zentrale Aussagen sowie Sprech- und Gesprächsabsichten),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/009aaf62-1763-4b46-bc52-5fc89cb08ffa -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/009aaf62-1763-4b46-bc52-5fc89cb08ffa">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">wenden erste Gesprächsstrategien an, z. B. Fragen stellen, etwas zusammenfassen, Rückmeldung geben,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/01afa893-c1a5-45be-8855-39a8f6088825 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/01afa893-c1a5-45be-8855-39a8f6088825">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/11977542-4eb9-42e5-bbb7-fc131a15238e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">nehmen in Gesprächen einfache Rollen ein (z. B. Zuhörerin bzw. Zuhören, Sprecherin bzw. Sprecher),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0261286d-a11b-46a0-a94a-62fd22d81e44 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0261286d-a11b-46a0-a94a-62fd22d81e44">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bd7b288d-2ce2-4123-adfb-25ea09b34960"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>1.1.3.5</skos:notation>
        <skos:prefLabel xml:lang="de">appellieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0446c36a-e062-4576-b4c7-e1e82f8522bc -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0446c36a-e062-4576-b4c7-e1e82f8522bc">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler erfahren, dass das Lesen von Büchern und Texten eine Auseinandersetzung mit der Welt ermöglicht und Vergnügen bereiten kann. Dazu werden an unterschiedlichen Texten Lesefreude, Leseinteresse und -bereitschaft, Lesefähigkeit und sinnverstehendes Lesen entwickelt. Beim Hören und Lesen literarischer Texte beschäftigen sich die Schüler und Schülerinnen mit wichtigen, sie bewegenden Fragen und setzen sich identifizierend und abgrenzend mit literarischen Figuren auseinander. Dies gilt auch für Theateraufführungen, bei denen sie zuschauen oder an denen sie mitwirken. Die Schülerinnen und Schüler gehen mit unterschiedlichen Textsorten in verschiedenen Medien um. Sie können sich ihrem Alter und Lernstand entsprechend im Medienangebot orientieren, d. h. in Druckmedien, in digitalen Medien und auditiven und audiovisuellen Medien, und ihren eigenen Lese- und Medieninteressen nachgehen. Sie lernen aus Texten und anderen Medien und können zu ihnen Stellung beziehen. Grundlegende Formen der Nutzung digitaler Medien sind ihnen vertraut. Der Deutschunterricht legt (auch in Kooperation mit anderen Fächern) das Fundament für einen zunehmend eigenständigen Umgang mit dem vielfältigen Medienangebot und leitet zur kritischen Reflexion der eigenen Mediennutzung an.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/d212d31c-fbeb-4484-b8e6-7d52c1bd840c"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1704f370-81c6-469e-a4dd-dea6137a1401"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/4150eebf-0aa6-4564-844b-274b0f015bec"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/79cec164-b3db-4e6b-b699-bbe871f5312f"/>
        <skos:notation>4</skos:notation>
        <skos:prefLabel xml:lang="de">Sich mit Texten und anderen Medien auseinandersetzen</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/05365fab-2bcc-425a-8a54-d911fec9a507 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/05365fab-2bcc-425a-8a54-d911fec9a507">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/4150eebf-0aa6-4564-844b-274b0f015bec"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>4.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">lesen selbstgewählte literarische Texte nach Vorbereitung sinngestaltend vor,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/0582a2fb-f2cf-478f-b627-269bb3740b1d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/0582a2fb-f2cf-478f-b627-269bb3740b1d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/c5f7e2b8-05c6-4f5f-a4e7-41c278b438ce"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.2.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Wörterbücher, Grammatiken und digitale Rechtschreibhilfen für das Nachschlagen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/1a0ed7aa-e37b-4c64-b384-0e34d2bf8669">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/0446c36a-e062-4576-b4c7-e1e82f8522bc"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler erschließen sich Texte unterschiedlicher medialer Form – auch als Grundlage für die mündliche und schriftliche Anschlusskommunikation.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/ac4f453f-aca9-48a6-b947-c45284651a6b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/cb213e6f-a192-4a3f-a74c-49b459653a0d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/ce6f3d24-2f6a-4fc9-886c-55b7240ff371"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/f0aac958-4d09-499f-87b4-f6deaa2be8ba"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/0c06f738-83c5-4560-8d2e-602dedec8d47"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/1f2a071d-178f-4d69-a16d-3be0d593252e"/>
