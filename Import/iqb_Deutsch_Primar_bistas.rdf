        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/bd25ba36-3e31-4e26-946c-5acf731b1fd9"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/c5425256-0f99-48d0-b6da-61ba507fe064"/>
