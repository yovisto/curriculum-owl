    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/263879a9-be69-49fb-bf28-b0a48ceab1f8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/263879a9-be69-49fb-bf28-b0a48ceab1f8">
