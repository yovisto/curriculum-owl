    <!-- https://w3id.org/iqb/bista22dep/bistas/33f483df-35af-4c5c-85ab-96d4d8b531b9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/33f483df-35af-4c5c-85ab-96d4d8b531b9">
