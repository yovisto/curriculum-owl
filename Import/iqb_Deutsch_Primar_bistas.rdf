    <!-- https://w3id.org/iqb/bista22dep/bistas/172a1261-0cba-48ea-b939-e589b92ac778 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/172a1261-0cba-48ea-b939-e589b92ac778">
