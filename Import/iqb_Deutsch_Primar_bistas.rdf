        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>2.2.8</skos:notation>
        <skos:prefLabel xml:lang="de">überprüfen und korrigieren Texte auf orthografische Richtigkeit.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/2ffad94a-282f-4f2d-9d4a-3e4245a56698 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/2ffad94a-282f-4f2d-9d4a-3e4245a56698">
