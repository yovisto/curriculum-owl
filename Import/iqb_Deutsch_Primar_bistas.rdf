        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22dep/bistas/"/>
        <skos:notation>5.2.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">tauschen sich über die Schreibung von Wörtern, Wortgruppen und Sätzen aus (z. B. Rechtschreibgespräch).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22dep/bistas/2253598e-6ee0-477e-98c4-151c7f29a1cb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22dep/bistas/2253598e-6ee0-477e-98c4-151c7f29a1cb">
