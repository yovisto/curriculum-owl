        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltMSA/"/>
        <skos:notation>4.8</skos:notation>
        <skos:prefLabel xml:lang="de">zeichnen und konstruieren geometrische Figuren unter Verwendung angemessener Medien wie Zirkel, Geodreieck oder digitaler Mathematikwerkzeuge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltMSA/659ec264-1687-4362-8f1a-99cc68a3b29e -->

