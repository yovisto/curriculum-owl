        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/79291abf-52a9-4ab3-93c5-4d225c3be38d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/85062c59-d3ed-496e-8ed2-cac5811917b0"/>
        <skos:prefLabel xml:lang="de">Inhaltsbezogene Kompetenzen ESA</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/a1886345-fc6e-4010-bdd5-5dd37dbbebd7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/a1886345-fc6e-4010-bdd5-5dd37dbbebd7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/e5eb3bbf-9934-418f-960a-f8789e56719f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>2.2</skos:notation>
        <skos:prefLabel xml:lang="de">wählen Einheiten von Größen situationsgerecht aus (insbesondere für Zeitspannen, Masse, Geld, Länge, Fläche, Volumen und Winkel) und wandeln sie ggf. um,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/a30faf32-80a9-4d3c-b441-5844af2fea88 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/a30faf32-80a9-4d3c-b441-5844af2fea88">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/85062c59-d3ed-496e-8ed2-cac5811917b0"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>4.1</skos:notation>
        <skos:prefLabel xml:lang="de">benennen und beschreiben geometrische Objekte und Beziehungen in der Umwelt mit Hilfe mathematischer Modelle (Punkte, Winkel, Strecken, Geraden, Flächen, Körper) und ihre Zusammenhänge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/a3595b2e-55e0-484d-804c-02ac88b337b6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/a3595b2e-55e0-484d-804c-02ac88b337b6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/e5eb3bbf-9934-418f-960a-f8789e56719f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>2.5</skos:notation>
        <skos:prefLabel xml:lang="de">ermitteln Volumen und Oberflächeninhalt von Quader, Prisma, Pyramide und Zylinder sowie daraus zusammengesetzten Körpern, auch mit Hilfe digitaler Mathematikwerkzeuge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/a97b7045-ef7e-4e1d-8c27-af5fed04f8e9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/a97b7045-ef7e-4e1d-8c27-af5fed04f8e9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/79291abf-52a9-4ab3-93c5-4d225c3be38d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>3.8</skos:notation>
        <skos:prefLabel xml:lang="de">lösen einfache lineare Gleichungen numerisch (systematisches Probieren), algebraisch (Umformen) und grafisch (mit Hilfe von Funktionsgraphen), auch unter Einsatz digitaler Mathematikwerkzeuge, und vergleichen verschiedene Lösungsverfahren im Hinblick auf die jeweilige Fragestellung oder das Problem,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/ac3db06f-47a6-4205-929d-52b34e1660dc -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/ac3db06f-47a6-4205-929d-52b34e1660dc">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/85062c59-d3ed-496e-8ed2-cac5811917b0"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>4.2</skos:notation>
        <skos:prefLabel xml:lang="de">entwickeln Vorstellungen im zwei- und dreidimensionalen Raum und operieren (insbesondere verschieben, drehen, spiegeln) gedanklich mit den darin enthaltenen Objekten (Punkten, Strecken, Flächen und Körpern),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/ad883a43-2acb-4360-b9f2-e160f214a2e7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/ad883a43-2acb-4360-b9f2-e160f214a2e7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/c0cc197e-34ab-40ed-9ffe-9a230b0c68ad"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>1.13</skos:notation>
        <skos:prefLabel xml:lang="de">erläutern an Beispielen den Zusammenhang zwischen Rechenoperationen und deren Umkehrungen und nutzen diese Zusammenhänge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/ae704375-93ed-4c3c-970e-df4851cc3791 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/ae704375-93ed-4c3c-970e-df4851cc3791">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/79291abf-52a9-4ab3-93c5-4d225c3be38d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>3.13</skos:notation>
        <skos:prefLabel xml:lang="de">interpretieren die Steigung linearer Funktionen im Sachzusammenhang,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/b9f806d2-5beb-4007-9a75-aa543bdb1b4c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/b9f806d2-5beb-4007-9a75-aa543bdb1b4c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/c0cc197e-34ab-40ed-9ffe-9a230b0c68ad"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>1.4</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen Zahlen nach ihren Faktoren, in einfachen Fällen ohne digitale Mathematikwerkzeuge,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/ba7d67d9-c976-4948-873d-0815732b0561 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/ba7d67d9-c976-4948-873d-0815732b0561">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/c0cc197e-34ab-40ed-9ffe-9a230b0c68ad"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>1.8</skos:notation>
        <skos:prefLabel xml:lang="de">erläutern an Beispielen die verschiedenen Vorstellungen zum Bruchbegriff (insbesondere Teile eines oder mehrerer Ganzer, relative Anteile),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/bcb874e9-27e0-4d6d-85b7-bf68b1ae4b4b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/bcb874e9-27e0-4d6d-85b7-bf68b1ae4b4b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/6b68cb04-9fd2-4b82-a7f0-4aae71f970fe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:notation>5.3</skos:notation>
        <skos:prefLabel xml:lang="de">planen einfache Befragungen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22mas1/stdInhaltESA/c0cc197e-34ab-40ed-9ffe-9a230b0c68ad -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22mas1/stdInhaltESA/c0cc197e-34ab-40ed-9ffe-9a230b0c68ad">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:definition xml:lang="de">Diese  Leitidee umfasst sinntragende Vorstellungen und Darstellungen von Zahlen und Operationen sowie die Nutzung von Rechengesetzen und Kontrollverfahren. Dazu gehört zum Beispiel die sachgerechte Nutzung von Prozent- und Zinsrechnung. Die darauf bezogenen mathematischen Sachgebiete der Sekundarstufe I sind die Arithmetik und Algebra</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22mas1/stdInhaltESA/ad883a43-2acb-4360-b9f2-e160f214a2e7"/>
