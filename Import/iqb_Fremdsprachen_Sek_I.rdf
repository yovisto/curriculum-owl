        <skos:notation>1.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">gebräuchliche Zeichen und Schilder an öffentlichen Orten, z.B. Wegweiser, Warnungen vor Gefahr verstehen (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/ea44065f-4f93-438d-b18d-b7002a0dfba9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/ea44065f-4f93-438d-b18d-b7002a0dfba9">
