        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">aus einfacheren schriftlichen Materialien wie Briefen, Broschüren, Zeitungsartikeln (oder auch dem Niveau entsprechenden fiktionalen Texten) spezifische Informationen herausfinden (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/d38044d5-4485-4dbb-8105-4bcb18d77382 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/d38044d5-4485-4dbb-8105-4bcb18d77382">
