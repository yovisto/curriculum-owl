        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">im Allgemeinen das Thema von Gesprächen, die in ihrer Gegenwart geführt werden, erkennen, wenn langsam und deutlich gesprochen wird (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/d306fc29-e086-4b34-8229-054ac4bf908e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/d306fc29-e086-4b34-8229-054ac4bf908e">
