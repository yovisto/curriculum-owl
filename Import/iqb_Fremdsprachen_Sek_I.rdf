        <skos:notation>2.2.7</skos:notation>
        <skos:prefLabel xml:lang="de">Bedingungen und Bezüge formulieren.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/65ce3286-1ee5-4f96-90f5-4b60053ab4bd -->

