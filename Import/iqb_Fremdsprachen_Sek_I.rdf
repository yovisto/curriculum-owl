        <skos:notation>1.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">die Aussagen einfacher literarischer Texte verstehen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/34e77a85-9a07-4b3c-a31f-e65b97e24cf9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/34e77a85-9a07-4b3c-a31f-e65b97e24cf9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
