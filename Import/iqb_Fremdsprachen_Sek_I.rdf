        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/643a71ba-8a2d-4ccd-9391-8b375d6f08dd"/>
        <skos:notation>4.2</skos:notation>
        <skos:prefLabel xml:lang="de">Interaktion</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/377e2166-b359-4a29-b1e9-af959ebacc23 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/377e2166-b359-4a29-b1e9-af959ebacc23">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
