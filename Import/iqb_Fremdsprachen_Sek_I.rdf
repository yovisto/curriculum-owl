        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">sind neugierig auf Fremdes, aufgeschlossen für andere Kulturen und akzeptieren kulturelle Vielfalt ohne Angst und Vorbehalte,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/f495708a-fc19-4ae0-9ba3-9bb83f165078 -->

