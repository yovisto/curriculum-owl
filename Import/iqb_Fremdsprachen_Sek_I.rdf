        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">kurze, einfache Notizen und Mitteilungen schreiben, die sich auf unmittelbare Bedürfnisse und notwendige Dinge beziehen (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/d397a211-40f0-4bb3-a374-7881fe457beb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/d397a211-40f0-4bb3-a374-7881fe457beb">
