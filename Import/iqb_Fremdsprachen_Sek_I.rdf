        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Zeichen der Lautschrift als Aussprachehilfe,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/f789b0b6-c653-42ca-9af2-3098cb696e2f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/f789b0b6-c653-42ca-9af2-3098cb696e2f">
