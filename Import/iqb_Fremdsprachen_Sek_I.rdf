        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.7</skos:notation>
        <skos:prefLabel xml:lang="de">können kulturelle Differenzen, Missverständnisse und Konfliktsituationen bewusst wahrnehmen, sich darüber verständigen und gegebenenfalls gemeinsam handeln.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/68df4803-ba2e-4de9-b363-178eb358d8f5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/68df4803-ba2e-4de9-b363-178eb358d8f5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
