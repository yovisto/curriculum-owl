        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">beherrschen die Aussprache im Allgemeinen klar genug, um verstanden zu werden; manchmal wird eine Wiederholung erforderlich,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/d398c5a2-5b5a-4c06-9884-b9c438a22a8f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/d398c5a2-5b5a-4c06-9884-b9c438a22a8f">
