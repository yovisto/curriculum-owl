        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/66204012-043a-4e13-b653-2b2829e8dadf"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/8481bb5a-81f7-475d-a03e-9ca9f2c03a69"/>
        <skos:notation>3.1</skos:notation>
        <skos:prefLabel xml:lang="de">Kompetenzen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/b0f22bb9-4738-4a95-90df-6133cee5a838 -->

