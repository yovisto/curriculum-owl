        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">sich in einfachen Routinesituationen (Einkaufen, Essen, öffentliche Verkehrsmittel) verständigen und Informationen geben und erfragen (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/f5559349-f9a0-431c-9766-7c16c5781752 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/f5559349-f9a0-431c-9766-7c16c5781752">
