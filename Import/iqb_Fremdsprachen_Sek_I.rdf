        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">um Entschuldigung bitten und auf Entschuldigungen reagieren (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/ee071eab-7dd4-4c32-9c07-e20ec3e6959a -->

