        <skos:notation>4.5.3</skos:notation>
        <skos:prefLabel xml:lang="de">Neue Technologien zur Informationsbeschaffung, zur kommunikativen Interaktion (E-Mail) und zur Präsentation der Ergebnisse nutzen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/1e8fd703-68a1-43bd-b539-df04ead7a3b4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/1e8fd703-68a1-43bd-b539-df04ead7a3b4">
