        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/380736fa-62e5-49b4-a722-fc74a4cd72ee"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/9a140af1-2a74-45f7-9deb-75388a15add8"/>
        <skos:notation>4.6</skos:notation>
        <skos:prefLabel xml:lang="de">Lernbewusstheit und Lernorganisation</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/80a38218-e171-4466-ab18-2dd2811fcdeb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/80a38218-e171-4466-ab18-2dd2811fcdeb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/6b4126c5-8f2c-4b65-afe8-8130f6010a0f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">Hilfsmittel zum Nachschlagen und Lernen, z. B. Wörterbücher, Grammatikbücher usw. selbstständig nutzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/8481bb5a-81f7-475d-a03e-9ca9f2c03a69 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/8481bb5a-81f7-475d-a03e-9ca9f2c03a69">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/adfe574c-c8bb-405e-83d6-96b85c4b2992"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">sind neugierig auf Fremdes, aufgeschlossen für andere Kulturen und akzeptieren kulturelle Vielfalt,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/855fec0e-c19e-4a46-993a-af30e4c03b25 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/855fec0e-c19e-4a46-993a-af30e4c03b25">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.6</skos:notation>
        <skos:prefLabel xml:lang="de">in klar geschriebenen argumentativen Texten zu vertrauten Themen die wesentlichen Schlussfolgerungen erkennen, z. B. in Zeitungsartikeln (B1/ B1+).</skos:prefLabel>
    </NamedIndividual>
    


