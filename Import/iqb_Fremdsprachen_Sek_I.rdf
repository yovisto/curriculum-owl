        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.2</skos:notation>
        <skos:prefLabel xml:lang="de">in persönlichen Briefen Mitteilungen, einfache Informationen und Gedanken darlegen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/d65aa242-27cc-47c0-9776-12d30a056af3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/d65aa242-27cc-47c0-9776-12d30a056af3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/323b34c6-9767-4f38-8ecc-68b64b7a1c18"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/b9bc0d08-c7d8-426b-8dcb-c95ccefee535"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/c503467b-52c9-4bba-b291-9f01c261fcec"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/c82a0a3d-ad40-455e-bc3d-fcb594066524"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/dbd93c09-4f59-416e-b05f-0cc48123c982"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/2da241b9-c5e8-432c-bf42-96f7c5e3a942"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/327c3ba8-58cd-4bce-b2ee-22580081bbf4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/77e7bfb4-3d43-4490-9b62-adf5b520bfc9"/>
        <skos:notation>4.6</skos:notation>
        <skos:prefLabel xml:lang="de">Lernbewusstheit und Lernorganisation</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/d7d7777d-3e59-4116-a8d4-f46d978735b5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/d7d7777d-3e59-4116-a8d4-f46d978735b5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">sind neugierig auf Fremdes, aufgeschlossen für andere Kulturen und akzeptieren kulturelle Vielfalt,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/d8c6968b-4a20-4d41-8c4b-924820a79c2f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/d8c6968b-4a20-4d41-8c4b-924820a79c2f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/24b9ee01-f378-4d6b-95c1-a4698758813d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">können die Zeichen der Lautschrift sprachlich umsetzen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/dbd93c09-4f59-416e-b05f-0cc48123c982 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/dbd93c09-4f59-416e-b05f-0cc48123c982">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
