        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.6.5</skos:notation>
        <skos:prefLabel xml:lang="de">Fehler erkennen und diese Erkenntnisse für den eigenen Lernprozess nutzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/dd771712-ea4c-4ca9-b75d-d8ffb2b5ec91 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/dd771712-ea4c-4ca9-b75d-d8ffb2b5ec91">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">sind in der Lage, ungewohnte Erfahrungen auszuhalten, mit ihnen sinnvoll und angemessen umzugehen und das Fremde nicht als etwas wahrzunehmen, das Angst macht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/eb5c588d-3eaa-40c3-bce2-1fa8d1fa4f0b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/eb5c588d-3eaa-40c3-bce2-1fa8d1fa4f0b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.3</skos:notation>
        <skos:prefLabel xml:lang="de">einfache standardisierte Briefe und E-Mails adressatengerecht formulieren, z. B. Anfragen, Bewerbungen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/ec12e454-d9fd-48df-b520-871b2bc72fce -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/ec12e454-d9fd-48df-b520-871b2bc72fce">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">sich schnell einen groben Überblick über den Inhalt eines Textes verschaffen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/ee050d54-da7f-4406-9588-3911e67279f9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/ee050d54-da7f-4406-9588-3911e67279f9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/0113a01f-396d-4041-8c8f-90ce26e6534a"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">sich in realen Alltagssituationen mit unterschiedlichen, englischsprachigen Personen verständigen und Verständigungsprobleme durch Rückfragen, durch Vereinfachungen, durch Höflichkeitsformeln und nonverbale Mittel überwinden,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f3a0d994-06d4-48a1-bf71-356addf2ea3a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f3a0d994-06d4-48a1-bf71-356addf2ea3a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae"/>
        <skos:definition xml:lang="de">Die sprachlichen Mittel Wortschatz, Grammatik, Aussprache, Intonation und Orthographie sind funktionale Bestandteile der Kommunikation. Anzustreben ist ihre höchstmögliche Verfügbarkeit, allerdings haben sie grundsätzlich dienende Funktion. Im Vordergrund steht die gelungene Kommunikation. Für die folgenden Bereiche werden keine detaillierten Listen angeführt. Die Entscheidung darüber bleibt den einzelnen Ländern vorbehalten. In diesem Zusammenhang sei auch auf die Ausführungen im Kapitel 5.2.1 – Linguistische Kompetenzen – des Gemeinsamen europäischen Referenzrahmens verwiesen (http://www.goethe.de/z/50/commeuro/deindex.htm). Bis zum Mittleren Schulabschluss erreichen die Schülerinnen und Schüler im Wesentlichen das Niveau B1 des Gemeinsamen europäischen Referenzrahmens.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/bfdcf12f-d676-4958-bd1a-cca2f3d95424"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/1503de73-c0a0-4eb7-9135-1e9591515504"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/24b9ee01-f378-4d6b-95c1-a4698758813d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:notation>2</skos:notation>
        <skos:prefLabel xml:lang="de">Verfügung über die sprachlichen Mittel</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f4790d5e-1db1-4fab-b30f-2ada1ce639d6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f4790d5e-1db1-4fab-b30f-2ada1ce639d6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/22db86ba-9268-4290-ab60-34441b85da81"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">Verfahren zur Vernetzung, Strukturierung, Memorierung und Speicherung von sprachlichen Inputs, z. B. von Wortschatz, anwenden.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f5da78dd-1daa-425f-8922-fef5bcc76831 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f5da78dd-1daa-425f-8922-fef5bcc76831">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/024c1435-9277-4c7f-a6a7-ea6c282bd672"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">sich Informationen aus unterschiedlichen fremdsprachlichen Textquellen beschaffen, die se vergleichen, auswählen und bearbeiten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f63edd84-8e23-44f4-86c4-45f0266e08bb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f63edd84-8e23-44f4-86c4-45f0266e08bb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.6</skos:notation>
        <skos:prefLabel xml:lang="de">in klar geschriebenen argumentativen Texten zu vertrauten Themen die wesentlichen Schlussfolgerungen erkennen, z. B. in Zeitungsartikeln (B1/ B1+).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f752f9ca-f6d0-4b29-a914-c5444dc51784 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f752f9ca-f6d0-4b29-a914-c5444dc51784">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Lebensbedingungen (Lebensstandard, geografische, soziokulturelle Merkmale, u.a.),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f7e0d5a7-cf11-4a1a-a3bd-644d3e387473 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f7e0d5a7-cf11-4a1a-a3bd-644d3e387473">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/024c1435-9277-4c7f-a6a7-ea6c282bd672"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Techniken des Notierens zur Vorbereitung eigener Texte oder Präsentationen nutzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f8a03e41-7a32-4baa-a2f2-532eb07a8f48 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f8a03e41-7a32-4baa-a2f2-532eb07a8f48">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">wichtige Textstellen durch farbliches Hervorheben, durch das Notieren von Stichworten und durch ordnende ergänzende Randnotizen besonders kenntlich machen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können weitgehend selbstständig verschiedene Texte aus Themenfeldern ihres Interessen- und Erfahrungsbereiches lesen und verstehen (B1+). Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f63edd84-8e23-44f4-86c4-45f0266e08bb"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/2ec32b6a-6c5f-4816-98f7-fe197728b31a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/327a76f9-f21e-4d9b-8c25-3cc2bcb5bd47"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/4eab9cb7-9e91-4b5b-bdf0-63677260cc34"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/510a5e80-66b0-4432-a237-eecf5624879c"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6518a1c4-fdc5-4345-80a6-566c65b84cb7"/>
        <skos:notation>1.2</skos:notation>
        <skos:prefLabel xml:lang="de">Leseverstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/0bf48a0c-1c82-45f7-ad68-91ff97e53eb2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/0bf48a0c-1c82-45f7-ad68-91ff97e53eb2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können mündlich in Routinesituationen und schriftlich zu vertrauten Themen zusammenhängende sprachliche Äußerungen und Texte sinngemäß von der einen in die andere Sprache übertragen. Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/a71cbbd0-747e-4ab5-9d88-aaa244ffc179"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/0d87b3d0-930a-4977-903e-4ccaeaf6e0b9"/>
        <skos:notation>1.6</skos:notation>
        <skos:prefLabel xml:lang="de">Sprachmittlung</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/0d87b3d0-930a-4977-903e-4ccaeaf6e0b9 -->

