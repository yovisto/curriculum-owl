        <skos:prefLabel xml:lang="de">Aussprache und Intonation</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/643a71ba-8a2d-4ccd-9391-8b375d6f08dd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/643a71ba-8a2d-4ccd-9391-8b375d6f08dd">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
