        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.6.2</skos:notation>
        <skos:prefLabel xml:lang="de">selbstständig, mit einem Partner oder in Gruppen längere Zeit arbeiten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/390f82a2-fec8-4951-80b8-e271d70dc2bb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/390f82a2-fec8-4951-80b8-e271d70dc2bb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/dd1f7079-738b-4ccf-91ff-2f7008bd4fb3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.6</skos:notation>
        <skos:prefLabel xml:lang="de">kurze Berichte zu vertrauten Themen schreiben, darin Informationen weitergeben, Gründe für Handlungen angeben und Stellung nehmen (B 1+).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7fc8d62d-feee-4817-97fc-8fc04e57e653"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können verschiedene unkomplizierte Texte aus Themenfeldern ihres Interessen- und Erfahrungsbereiches lesen und verstehen (B1).</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/b2b33ac5-07ea-4a1c-bc54-adb5003b0f22"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/3eadee42-a8cb-43de-94ee-f4f574c0ff9d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/5be08035-7d7d-4ab4-99ee-01e1ba270aef"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/68df4803-ba2e-4de9-b363-178eb358d8f5"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7f2e95a2-7045-4b00-93cb-8bf6cc5f0295"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/855fec0e-c19e-4a46-993a-af30e4c03b25"/>
        <skos:notation>1.2</skos:notation>
        <skos:prefLabel xml:lang="de">Leseverstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/46bfc49b-0ccf-49d9-b499-6c8fcd27d6ff -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/46bfc49b-0ccf-49d9-b499-6c8fcd27d6ff">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/f73b160c-a635-481b-af2d-01134dfc6322"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Lebensbedingungen (Lebensstandard, geografische, soziokulturelle Merkmale, u.a.),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/4619dc14-240a-4b37-9a51-20f808c85f73 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/4619dc14-240a-4b37-9a51-20f808c85f73">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/266ac6eb-07a5-4671-bb0c-10aaba01cba9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">soziale Kontakte herstellen durch Begrüßung, Abschied, Sich-Vorstellen, Danken und Höflichkeitsformeln verwenden (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/463ce74d-204c-4234-a297-731b44cdc6ba -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/463ce74d-204c-4234-a297-731b44cdc6ba">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/266ac6eb-07a5-4671-bb0c-10aaba01cba9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">eine kurze Geschichte, einen Artikel, einen Vortrag, ein Interview oder eine Dokumentarsendung zu vertrauten Themen einem Gesprächspartner vorstellen und Informationsfragen dazu beantworten (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/5b92776d-5098-4759-8424-c6ef7550fd3c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/5b92776d-5098-4759-8424-c6ef7550fd3c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/dd1f7079-738b-4ccf-91ff-2f7008bd4fb3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.3</skos:notation>
        <skos:prefLabel xml:lang="de">einfache standardisierte Briefe und E-Mails adressatengerecht formulieren, z. B. Anfragen, Bewerbungen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/5be08035-7d7d-4ab4-99ee-01e1ba270aef -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/5be08035-7d7d-4ab4-99ee-01e1ba270aef">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">in kürzeren literarischen Texten (z. B. Short Stories) die wesentlichen Aussagen erfassen und diese zusammentragen, um eine bestimmte Aufgabe zu lösen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/5e0be73f-15fb-4190-a295-902a546432af -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/5e0be73f-15fb-4190-a295-902a546432af">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/3457ac2a-b154-41e4-a78c-9019d624bbfe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">Techniken des Vermittelns zwischen zwei Sprachen einsetzen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/5eac0bc9-d055-4d91-8ee6-bbeefe8c0738 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/5eac0bc9-d055-4d91-8ee6-bbeefe8c0738">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/266ac6eb-07a5-4671-bb0c-10aaba01cba9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Gefühle wie Überraschung, Freude, Trauer, Interesse und Gleichgültigkeit ausdrücken und auf entsprechende Gefühlsäußerungen reagieren (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/520ae28d-9510-429c-8520-22be03ca5e61 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/520ae28d-9510-429c-8520-22be03ca5e61">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7fc8d62d-feee-4817-97fc-8fc04e57e653"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können unkomplizierte Sachinformationen über gewöhnliche alltags- oder berufsbezogene Themen verstehen und dabei die Hauptaussagen und Einzelinformationen erkennen, wenn in deutlich artikulierter Standardsprache gesprochen wird (B1+). Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/c60cc64f-6a34-4482-970c-6e16b15e6377"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/deb42cf2-cd4f-447e-a5c4-552e0bf1d434"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/15a47d01-9dd5-4dd6-a6ee-72b6aa065532"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/75b1aad8-ec3a-41f6-817e-9496078e50ec"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7556fe8a-afb0-4ab9-94de-dd673807e218"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/9121f3ad-a7db-4095-afcb-685ab9ee8ff9"/>
        <skos:notation>1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Hör- und Hör-/Sehverstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/5280a3eb-62fd-445a-b6d5-f23c636c3ab0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/5280a3eb-62fd-445a-b6d5-f23c636c3ab0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/c6e742f9-34b3-421f-8ed2-e035ea1b7fb8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.5.3</skos:notation>
        <skos:prefLabel xml:lang="de">Neue Technologien zur Informationsbeschaffung, zur kommunikativen Interaktion (E-Mail) und zur Präsentation der Ergebnisse nutzen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/57410755-cf33-441d-bcd0-f2c113ea4852 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/57410755-cf33-441d-bcd0-f2c113ea4852">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/adfe574c-c8bb-405e-83d6-96b85c4b2992"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">sind bereit, sich auf fremde Situationen einzustellen und sich in Situationen des Alltagslebens angemessen zu verhalten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/58c7c196-068b-41ab-bffe-7c54d3530f30 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/58c7c196-068b-41ab-bffe-7c54d3530f30">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/6aa14696-ba38-40d0-b0f3-c7c3493216f2"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Handlungen, Ereignisse und Sachverhalte als gegenwärtig, vergangen, zukünftig oder zeitlos, mehrere Geschehnisse als gleichzeitig oder aufeinanderfolgend bzw. unter Berücksichtigung von Vor- und Nachzeitigkeit erkennen und wiedergeben,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/6a9fcdac-25ea-4964-aad4-794904e6e3ad -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/6a9fcdac-25ea-4964-aad4-794904e6e3ad">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/3457ac2a-b154-41e4-a78c-9019d624bbfe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">sich in realen Alltagssituationen mit unterschiedlichen, französischsprachigen Personen verständigen und Verständigungsprobleme durch Rückfragen, durch Vereinfachungen, durch Höflichkeitsformeln und nonverbale Mittel überwinden,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/6aa14696-ba38-40d0-b0f3-c7c3493216f2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/6aa14696-ba38-40d0-b0f3-c7c3493216f2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/bea93373-bd2d-4f48-9bb1-e8b34364c613"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/a31b0788-e2ee-4680-8dd6-d27229bb54a8"/>
