        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Korrespondenz lesen, die sich auf das eigene Interessengebiet bezieht und die wesentliche Aussage erfassen (B2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/68e9738a-0d0d-4803-8eca-cc016bb430b4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/68e9738a-0d0d-4803-8eca-cc016bb430b4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/76d8bc7a-e56f-4149-b330-9be19dd42ce4"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">mit einfachen Mitteln Gegenstände und Vorgänge des Alltags beschreiben, z. B. Rezepte, Wegbeschreibungen, Spielregeln, Bedienungsanleitungen (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/7e28aca2-fc4d-40c4-8ad9-41eca24b3442 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/7e28aca2-fc4d-40c4-8ad9-41eca24b3442">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/76d8bc7a-e56f-4149-b330-9be19dd42ce4"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">eine vorbereitete Präsentation zu einem vertrauten Thema vortragen, wobei die Hauptpunkte hinreichend präzise erläutert werden (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/7f2e95a2-7045-4b00-93cb-8bf6cc5f0295 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/7f2e95a2-7045-4b00-93cb-8bf6cc5f0295">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">längere Texte nach gewünschten Informationen durchsuchen und Informationen aus verschiedenen Texten zusammentragen, um eine bestimmte Aufgabe zu lösen (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/7fc8d62d-feee-4817-97fc-8fc04e57e653 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/7fc8d62d-feee-4817-97fc-8fc04e57e653">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf"/>
        <skos:definition xml:lang="de">Bis zum Mittleren Schulabschluss erreichen die Schülerinnen und Schüler im Wesentlichen das Niveau B1 des Gemeinsamen europäischen Referenzrahmens. In Abhängigkeit von den Eigenbedingungen schulischen Fremdsprachenlernens sind jedoch in einigen Kompetenzbereichen höhere oder niedrigere Anforderungsniveaus etabliert, die hier entsprechend berücksichtigt wurden (z. B. mit der Ausweisung von B1+ oder A2). Das Niveau des Referenzrahmens – soweit dort ausgewiesen – ist für jeden Standard in Klammern angegeben.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/a4a79ef7-bf20-4663-b568-fc55ed4403b2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/dd1f7079-738b-4ccf-91ff-2f7008bd4fb3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/266ac6eb-07a5-4671-bb0c-10aaba01cba9"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/41a1cbc4-787d-41a3-9a44-e284e213392d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/520ae28d-9510-429c-8520-22be03ca5e61"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/76d8bc7a-e56f-4149-b330-9be19dd42ce4"/>
        <skos:notation>1</skos:notation>
        <skos:prefLabel xml:lang="de">Kommunikative Fertigkeiten</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/75b1aad8-ec3a-41f6-817e-9496078e50ec -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/75b1aad8-ec3a-41f6-817e-9496078e50ec">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/520ae28d-9510-429c-8520-22be03ca5e61"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">Ankündigungen und Mitteilungen zu konkreten Themen verstehen, die in normaler Geschwindigkeit in Standardsprache gesprochen werden (B2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/7556fe8a-afb0-4ab9-94de-dd673807e218 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/7556fe8a-afb0-4ab9-94de-dd673807e218">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/520ae28d-9510-429c-8520-22be03ca5e61"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">in Radionachrichten und in einfacheren Tonaufnahmen über vertraute Themen die Hauptpunkte verstehen, wenn relativ langsam und deutlich gesprochen wird (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/76d8bc7a-e56f-4149-b330-9be19dd42ce4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/76d8bc7a-e56f-4149-b330-9be19dd42ce4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7fc8d62d-feee-4817-97fc-8fc04e57e653"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können Erfahrungen und Sachverhalte zusammenhängend darstellen, z. B. beschreiben, berichten, erzählen und bewerten (B1). Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/de1035a5-5211-40dc-be5d-d96e0ebab420"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/68e9738a-0d0d-4803-8eca-cc016bb430b4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/7e28aca2-fc4d-40c4-8ad9-41eca24b3442"/>
        <skos:notation>1.4</skos:notation>
        <skos:prefLabel xml:lang="de">Sprechen - Zusammenhängendes Sprechen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msaf/78b41122-a646-4f62-be43-50c89387632d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msaf/78b41122-a646-4f62-be43-50c89387632d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/ca899bc7-7ef6-41b7-bb46-f27e8fda1ddb"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/e392e71e-ffad-4001-938f-107025254173"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/0ad5b23e-55f3-42b5-851c-e48956700626"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msaf/0fa171ac-28a7-4d6f-b314-7be8a5fdd437"/>
