        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.4</skos:notation>
        <skos:prefLabel xml:lang="de">unkomplizierte, detaillierte Texte zu einer Reihe verschiedener Themen aus ihren Interessengebieten verfassen, z. B. Erfahrungsberichte, Geschichten, Beschreibungen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/277eeb07-7c13-4dbb-a2b7-3cc9da5424a5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/277eeb07-7c13-4dbb-a2b7-3cc9da5424a5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">können sich in Bezug auf die Befindlichkeiten und Denkweisen in den fremdkulturellen Partner hineinversetzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/323b34c6-9767-4f38-8ecc-68b64b7a1c18 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/323b34c6-9767-4f38-8ecc-68b64b7a1c18">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler verfügen ihrem Alter und Bildungsgang entsprechend über fachliche und fachübergreifende Arbeitstechniken und Methoden.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/d65aa242-27cc-47c0-9776-12d30a056af3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/0113a01f-396d-4041-8c8f-90ce26e6534a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/024c1435-9277-4c7f-a6a7-ea6c282bd672"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/22db86ba-9268-4290-ab60-34441b85da81"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd"/>
        <skos:notation>4</skos:notation>
        <skos:prefLabel xml:lang="de">Methodenkompetenzen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/327a76f9-f21e-4d9b-8c25-3cc2bcb5bd47 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/327a76f9-f21e-4d9b-8c25-3cc2bcb5bd47">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">die Aussagen einfacher literarischer Texte verstehen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/327c3ba8-58cd-4bce-b2ee-22580081bbf4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/327c3ba8-58cd-4bce-b2ee-22580081bbf4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/d65aa242-27cc-47c0-9776-12d30a056af3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.6.7</skos:notation>
        <skos:prefLabel xml:lang="de">Methoden des Spracherwerbs reflektieren und diese auf das Lernen weiterer Sprachen übertragen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können unkomplizierte Sachinformationen über gewöhnliche alltags- oder berufsbezogene Themen verstehen und dabei die Hauptaussagen und Einzelinformationen erkennen, wenn in deutlich artikulierter Standardsprache gesprochen wird (B1+). Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/1b7b7741-c300-40f9-9c97-b5f129d45fb2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/1115d485-6d2c-4046-8e0b-8c7c80b0fc5b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/4d91f127-9d7a-429d-b1e0-449783af1145"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/7a15151d-992c-4471-90ce-122acb24f9fc"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/70b1bc0b-4168-4d32-a5be-699d1b84403f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/819a771e-8a15-4cf2-86b9-62ee8f89f525"/>
        <skos:notation>1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Hör- und Hör-/Sehverstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/399fccb2-a724-4838-abba-adf6dbbfb479 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/399fccb2-a724-4838-abba-adf6dbbfb479">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/24b9ee01-f378-4d6b-95c1-a4698758813d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">können verschiedenartige Aussprachevarianten der Zielsprache verstehen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/4d91f127-9d7a-429d-b1e0-449783af1145 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/4d91f127-9d7a-429d-b1e0-449783af1145">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.6</skos:notation>
        <skos:prefLabel xml:lang="de">das Wesentliche in vielen Fernsehsendungen zu Themen von persönlichem Interesse, z. B. Interviews, kurze Vorträge oder Nachrichtensendungen verstehen (B1+).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/4eab9cb7-9e91-4b5b-bdf0-63677260cc34 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/4eab9cb7-9e91-4b5b-bdf0-63677260cc34">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">klar formulierte Anweisungen, unkomplizierte Anleitungen, Hinweise und Vorschriften verstehen (B1/ B2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/d1043ac8-d9e4-4c57-8661-d110c10610bb"/>
        <skos:definition xml:lang="de">Die Kenntnisse und Fertigkeiten beziehen sich insbesondere auf Charakteristika der eigenen und der fremdsprachlichen Gesellschaft und Kultur aus folgenden Bereichen:</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f752f9ca-f6d0-4b29-a914-c5444dc51784"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/2e4fbe68-5eed-4562-9f3e-9c4f9a3f4303"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/2f561421-4245-452d-a24c-5ee632f0b702"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/64193303-50f9-4471-8056-578763fe3c94"/>
        <skos:notation>3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Bezüge der Kompetenzen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/43e2e3b5-b7d2-4c92-82b4-aae72d36966e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/43e2e3b5-b7d2-4c92-82b4-aae72d36966e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">verschiedene Hör- und Lesetechniken auf unterschiedliche Textarten (z. B. Sachtexte, Artikel, literarische Kleinformen) anwenden,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/48288907-fb19-4113-92ad-6be847ad4d19 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/48288907-fb19-4113-92ad-6be847ad4d19">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">kennen elementare spezifische Kommunikations- und Interaktionsregeln ausgewählter englischsprachiger Länder und verfügen über ein entsprechendes Sprachregister, das sie in vertrauten Situationen anwenden können,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/49687108-e732-46ea-a282-682eb55c441e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/49687108-e732-46ea-a282-682eb55c441e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">Präsentationstechniken einsetzen (Medienwahl, Gliederungstechniken, Visualisierungstechniken, Gruppenpräsentation),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/4995bf44-16b4-4887-95ad-ff10a35074df -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/4995bf44-16b4-4887-95ad-ff10a35074df">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">eine kurze Geschichte, einen Artikel, einen Vortrag, ein Interview oder eine Dokumentarsendung zu vertrauten Themen einem Gesprächspartner vorstellen und Informationsfragen dazu beantworten (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/510a5e80-66b0-4432-a237-eecf5624879c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/510a5e80-66b0-4432-a237-eecf5624879c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">in kürzeren literarischen Texten (z. B. Short Stories) die wesentlichen Aussagen erfassen und diese zusammentragen, um eine bestimmte Aufgabe zu lösen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können zusammenhängende Texte zu vertrauten Themen aus ihrem Interessengebiet verfassen (B1). Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/d1a180ee-8cea-49bb-878d-16d90ef56f27"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/eb5c588d-3eaa-40c3-bce2-1fa8d1fa4f0b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/1f366a8b-9370-4ca1-9a77-03081363d01d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/274d00fd-dd90-4e7a-968b-ef6db7d2e3dc"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/7c9b486d-7456-434a-9164-aa4926ec92f8"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/818c2864-e142-4cc5-b4ff-4b5a43b6ec5f"/>
        <skos:notation>1.5</skos:notation>
        <skos:prefLabel xml:lang="de">Schreiben</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/55c5b77f-b2d4-4c13-a611-4d13690eef73">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae"/>
        <skos:definition xml:lang="de">Bis zum Mittleren Schulabschluss erreichen die Schülerinnen und Schüler im Wesentlichen das Niveau B1 des Gemeinsamen europäischen Referenzrahmens. In Abhängigkeit von den Eigenbedingungen schulischen Fremdsprachenlernens sind jedoch in einigen Kompetenzbereichen höhere oder niedrigere Anforderungsniveaus etabliert, die hier entsprechend berücksichtigt wurden (z. B. mit der Ausweisung von B1+ oder A2). Das Niveau des Referenzrahmens – soweit dort ausgewiesen – ist für jeden Standard in Klammern angegeben.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/0bf48a0c-1c82-45f7-ad68-91ff97e53eb2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e835887-30be-4231-9af1-2514af3850e5"/>
        <skos:notation>1</skos:notation>
        <skos:prefLabel xml:lang="de">Kommunikative Fertigkeiten</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/6d7678be-c623-407b-a2b4-52abf2969387 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/6d7678be-c623-407b-a2b4-52abf2969387">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">Anzahl, Art und Zugehörigkeit von Gegenständen, Lebewesen und Sachverhalten erkennen und beschreiben,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/6e1c30fa-7d6b-4466-89d7-e7d2d06c13bc -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/6e1c30fa-7d6b-4466-89d7-e7d2d06c13bc">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Gefühle wie Überraschung, Freude, Trauer, Interesse und Gleichgültigkeit ausdrücken und auf entsprechende Gefühlsäußerungen reagieren (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/323b34c6-9767-4f38-8ecc-68b64b7a1c18"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/ec12e454-d9fd-48df-b520-871b2bc72fce"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f8a03e41-7a32-4baa-a2f2-532eb07a8f48"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/43e2e3b5-b7d2-4c92-82b4-aae72d36966e"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/77dac0e2-d09c-4bc1-8496-2e3dfc938021"/>
        <skos:notation>4.1</skos:notation>
        <skos:prefLabel xml:lang="de">Textrezeption (Leseverstehen und Hörverstehen)</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/62f100bc-526a-4020-a1e7-58fc4afd5525 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/62f100bc-526a-4020-a1e7-58fc4afd5525">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Handlungen, Ereignisse und Sachverhalte als gegenwärtig, vergangen, zukünftig oder zeitlos, mehrere Geschehnisse als gleichzeitig oder aufeinanderfolgend bzw. unter Berücksichtigung von Vor- und Nachzeitigkeit erkennen und wiedergeben,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/64193303-50f9-4471-8056-578763fe3c94 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/64193303-50f9-4471-8056-578763fe3c94">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>3.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">Werte, Normen, Überzeugungen, Einstellungen (in Bezug auf regionale Kulturen, Traditionen, Geschichte, Minderheiten, Kunst, u.a.).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/6518a1c4-fdc5-4345-80a6-566c65b84cb7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/6518a1c4-fdc5-4345-80a6-566c65b84cb7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/f9da45d2-7a32-4950-9197-78e65938b7b7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">längere Texte nach gewünschten Informationen durchsuchen und Informationen aus verschiedenen Texten zusammentragen, um eine bestimmte Aufgabe zu lösen (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/66cb2b74-6ea9-4727-b4d5-73614b826875 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/66cb2b74-6ea9-4727-b4d5-73614b826875">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">räumliche, zeitliche und logische Beziehungen erkennen und herstellen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/684a2aa5-7663-4130-bab8-d8617bfe912b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/684a2aa5-7663-4130-bab8-d8617bfe912b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/024c1435-9277-4c7f-a6a7-ea6c282bd672"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">mit Hilfe von Stichworten, Gliederungen, Handlungsgeländern Texte mündlich vortragen oder schriftlich verfassen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/7a15151d-992c-4471-90ce-122acb24f9fc -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/7a15151d-992c-4471-90ce-122acb24f9fc">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">den Informationsgehalt der meisten Rundfunksendungen und Tonaufnahmen über Themen von persönlichem Interesse verstehen (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/7a932ddf-93e1-49e7-9707-ba9033695b4a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/7a932ddf-93e1-49e7-9707-ba9033695b4a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">soziale Kontakte herstellen durch Begrüßung, Abschied, Sich-Vorstellen, Danken und Höflichkeitsformeln verwenden (A2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/7c434f12-9e68-4a6e-a3dc-cf37240241d0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/7c434f12-9e68-4a6e-a3dc-cf37240241d0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Aussagen, Fragen und Aufforderungen in bejahter und verneinter Form verstehen und formulieren,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/7c9b486d-7456-434a-9164-aa4926ec92f8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/7c9b486d-7456-434a-9164-aa4926ec92f8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.6</skos:notation>
        <skos:prefLabel xml:lang="de">kurze Berichte zu vertrauten Themen schreiben, darin Informationen weitergeben, Gründe für Handlungen angeben und Stellung nehmen (B 1+).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/7e8dd970-010e-4e53-ad2b-48d58a88e7da -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/7e8dd970-010e-4e53-ad2b-48d58a88e7da">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/22db86ba-9268-4290-ab60-34441b85da81"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">Hilfsmittel zum Nachschlagen und Lernen, z. B. Wörterbücher, Grammatikbücher usw. selbstständig nutzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/70b1bc0b-4168-4d32-a5be-699d1b84403f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/70b1bc0b-4168-4d32-a5be-699d1b84403f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">Ankündigungen und Mitteilungen zu konkreten Themen verstehen, die in normaler Geschwindigkeit in Standardsprache gesprochen werden (B2),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/748776b8-cd8a-4197-8ada-7af601146d1a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/748776b8-cd8a-4197-8ada-7af601146d1a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/24b9ee01-f378-4d6b-95c1-a4698758813d"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">beherrschen die Aussprache in der Weise, dass diese i.d.R. weder auf der Wort- noch auf der Satzebene zu Missverständnissen führt,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/323b34c6-9767-4f38-8ecc-68b64b7a1c18"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/1c19606d-6c5c-4c6b-bdda-a537010248b1"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/49687108-e732-46ea-a282-682eb55c441e"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e010231-1e94-4bab-9383-52e05f47555f"/>
        <skos:notation>4.5</skos:notation>
        <skos:prefLabel xml:lang="de">Präsentation und Medien</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/76c58d08-cf00-4213-9136-43fa0619223e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/76c58d08-cf00-4213-9136-43fa0619223e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/908292d5-2247-4400-a1c2-c2c1f6f3f696"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>2.2.6</skos:notation>
        <skos:prefLabel xml:lang="de">Informationen wörtlich und vermittelt wiedergeben (direkte/indirekte Rede),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/77dac0e2-d09c-4bc1-8496-2e3dfc938021 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/77dac0e2-d09c-4bc1-8496-2e3dfc938021">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/6f0bd5e8-7220-4306-8a12-de9637a47e68"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">wichtige Details durch Unterstreichen markieren,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/77e7bfb4-3d43-4490-9b62-adf5b520bfc9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/77e7bfb4-3d43-4490-9b62-adf5b520bfc9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/d65aa242-27cc-47c0-9776-12d30a056af3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>4.6.2</skos:notation>
        <skos:prefLabel xml:lang="de">selbstständig, mit einem Partner oder in Gruppen längere Zeit arbeiten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/79f8a6a9-a953-4827-b5a7-b1a8f1f1e189 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/79f8a6a9-a953-4827-b5a7-b1a8f1f1e189">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">die meisten Dienstleistungsgespräche und routinemäßigen Situationen bewältigen, z. B. Umgang mit öffentlichen Einrichtungen während eines Auslandsaufenthaltes, Einkauf, Essen (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/8efe754d-69c1-4c2c-97c1-82b7a9f766dd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/8efe754d-69c1-4c2c-97c1-82b7a9f766dd">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e835887-30be-4231-9af1-2514af3850e5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">eine vorbereitete Präsentation zu einem vertrauten Thema vortragen, wobei die Hauptpunkte hinreichend präzise erläutert werden (B1),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/818c2864-e142-4cc5-b4ff-4b5a43b6ec5f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/818c2864-e142-4cc5-b4ff-4b5a43b6ec5f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/55b5b07b-753e-44bc-845f-38c86d25b15f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">eine Nachricht notieren, wenn jemand nach Informationen fragt oder ein Problem erläutert (B1+),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/819a771e-8a15-4cf2-86b9-62ee8f89f525 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/819a771e-8a15-4cf2-86b9-62ee8f89f525">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/395c1569-a353-4cd5-bd95-3567528dc4a5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">vielen Filmen folgen, deren Handlung im Wesentlichen durch Bild und Aktion getragen wird (B1).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/msae/82797d35-f595-4ff4-b510-dd36049a91a2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/msae/82797d35-f595-4ff4-b510-dd36049a91a2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/msae/9e02c338-d884-4787-9690-0cc45a194948"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista04-enfr-s1/"/>
        <skos:notation>1.3.5</skos:notation>
        <skos:prefLabel xml:lang="de">in einem Interview konkrete Auskünfte geben, z. B. in Bewerbungsgesprächen (B1+),</skos:prefLabel>
    </NamedIndividual>
    


