        <skos:notation>4.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">einfache Techniken des Vermittelns zwischen zwei Sprachen einsetzen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/f3587e3e-8432-4a96-b3f0-4adae2179529 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista04-enfr-s1/esa/f3587e3e-8432-4a96-b3f0-4adae2179529">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
