        <skos:notation>1.3.8</skos:notation>
        <skos:prefLabel xml:lang="de">in einem Interview einfache Fragen beantworten und auf einfache Feststellungen reagieren (A2).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista04-enfr-s1/esa/d447b076-213e-4f82-90b2-1a90eb52d89d -->

