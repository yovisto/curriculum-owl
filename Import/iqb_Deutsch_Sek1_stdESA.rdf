        <skos:notation>1.4</skos:notation>
        <skos:prefLabel xml:lang="de">Vor anderen sprechen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/87b00bf2-e98e-46bf-b17e-83b44c9a9b11 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/87b00bf2-e98e-46bf-b17e-83b44c9a9b11">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
