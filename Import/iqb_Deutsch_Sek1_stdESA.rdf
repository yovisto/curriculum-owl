        <skos:notation>2.5.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">Appellieren: Argumentieren auf Basis klar strukturierter Material- und Textquellen (argumentierend materialgestützt schreiben)</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/9d918410-929a-4163-8160-22c665cfd5c2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/9d918410-929a-4163-8160-22c665cfd5c2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
