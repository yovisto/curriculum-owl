        <skos:notation>3.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">lesen komplexere Texte nach Vorbereitung sinngestaltend vor,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/a76b29cb-c0a7-476e-bd20-14f15a9f582a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/a76b29cb-c0a7-476e-bd20-14f15a9f582a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
