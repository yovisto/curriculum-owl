        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.2.7</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Rechtschreibhilfen (Wörterbuch und digitale Rechtschreibhilfe),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79">
