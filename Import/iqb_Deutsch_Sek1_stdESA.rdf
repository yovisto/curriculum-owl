    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/93637534-243a-42d3-9251-6d6ffd90a7f7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/93637534-243a-42d3-9251-6d6ffd90a7f7">
