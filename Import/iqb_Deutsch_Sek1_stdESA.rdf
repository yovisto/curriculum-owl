        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.5.6</skos:notation>
        <skos:prefLabel xml:lang="de">beziehen an ausgewählten Beispielen Textaussagen auf eigene Welt- und Wertvorstellungen und überprüfen diese,</skos:prefLabel>
    </NamedIndividual>
    


