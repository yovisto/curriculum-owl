        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen Wörter und Wendungen in ihrer Struktur und hinsichtlich ihrer Verwendungsbedingungen, ihrer Bedeutung und ihrer Beziehungen zu anderen Wörtern,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8197e8bf-54d8-4e84-bbd2-2e94e7ddc149 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/8197e8bf-54d8-4e84-bbd2-2e94e7ddc149">
