        <skos:notation>2.2.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden zwischen Kernwörtern und Fremdwörtern,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/a4bc1616-8611-40e6-8417-a26e2407fc35 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/a4bc1616-8611-40e6-8417-a26e2407fc35">
