        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e35da71c-cf4f-47a7-bf19-aaa29ec5aa77"/>
