    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/a6405631-c41c-49cd-b9a8-6bbf7da3fe9f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/a6405631-c41c-49cd-b9a8-6bbf7da3fe9f">
