        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/bf2f3ac3-d450-4914-8040-7fa834e40f76"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c3319518-2c59-4ec4-88f5-36fb1e171bab"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/29d2f2e4-9e70-4db3-9abc-0ec65ffdec63"/>
