        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/70d79fbd-d094-4aa7-99da-377c830b1cd3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8bf21d6f-5c33-4345-a6eb-f4e0da0dd8c3"/>
        <skos:notation>2.4</skos:notation>
        <skos:prefLabel xml:lang="de">Texte planen und strukturieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/500c9e73-2679-4241-affb-8caf6aec766d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/500c9e73-2679-4241-affb-8caf6aec766d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d5558e46-feb6-4a89-ba94-75b51b546baa"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.5.4</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden informierende, erzählende und appellierende Elemente in Texten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/52eb0d23-cd08-4667-89f8-c4bd3b6f7b59 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/52eb0d23-cd08-4667-89f8-c4bd3b6f7b59">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.2</skos:notation>
        <skos:prefLabel xml:lang="de">formulieren Texte auch für unbekannte und unterschiedliche Adressaten angemessen, </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/52f8db60-09f3-4ad6-9c0f-d5af81169361 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/52f8db60-09f3-4ad6-9c0f-d5af81169361">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
