        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5ef786f2-8308-4151-b5de-daa241ff7dfb"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5701bce3-62b9-459e-8497-82d0e7b3f4d8"/>
        <skos:notation>5.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">Mündliche und schriftliche Kommunikation</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/222b2cf3-50cf-45e3-b3a0-da8a09744e4f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/222b2cf3-50cf-45e3-b3a0-da8a09744e4f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c5276f76-7440-496d-a034-17301994bc9f"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler erschließen sich Texte unterschiedlicher medialer Form – auch als Grundlage für die mündliche und schriftliche Anschlusskommunikation.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.3</skos:notation>
        <skos:prefLabel xml:lang="de">Texte in unterschiedlicher medialer Form erschließen und nutzen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/23e368b0-9822-445d-98da-473f9edc37c5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/23e368b0-9822-445d-98da-473f9edc37c5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e14d64c2-a0a7-48e6-a7c6-954960023d08"/>
        <skos:notation>5.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">Ressourcen zur deutschen Sprache</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/23ff64ec-87ac-43f5-a194-896ce0d61e1e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/23ff64ec-87ac-43f5-a194-896ce0d61e1e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b912ae89-9eff-46d6-bea8-d372ae33bc01"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler schreiben flüssig; eigene Gedanken formulieren sie weitgehend flüssig. Sie schreiben in einer leserlichen Handschrift und mithilfe digitaler Schreibwerkzeuge in einem der Situation angemessenen Tempo und gestalten ihre Texte zweckmäßig und übersichtlich sowie sinnvoll strukturiert.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c367dcd6-4c05-49dc-b71f-9437aa164112"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/1cc6fdb3-c357-4ebe-b79c-882920c0a9d8"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8f1d5ba1-ff00-45b0-a122-5cd2a390c386"/>
        <skos:notation>2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Über Schreibfertigkeiten verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/24da74be-a6d2-4b8f-8e73-d5e1c3016dc2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/24da74be-a6d2-4b8f-8e73-d5e1c3016dc2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.11</skos:notation>
        <skos:prefLabel xml:lang="de">gestalten Texte unterschiedlicher medialer Form szenisch,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/27f070fa-bb81-4a63-b008-fb9e532d3deb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/27f070fa-bb81-4a63-b008-fb9e532d3deb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c5276f76-7440-496d-a034-17301994bc9f"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler nutzen das Medienangebot zur Entwicklung und Pflege eigener Interessen, zur Erweiterung ihrer Wissensbasis und Urteilsfähigkeit und als Anlass zum Austausch mit anderen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/cdd1239f-a7fd-4189-85b5-453a8d60c7b7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/780818e3-1245-4ded-a298-ef2aeff075ab"/>
        <skos:notation>4.2</skos:notation>
        <skos:prefLabel xml:lang="de">Sich im Medienangebot orientieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/29be5b1f-f84f-4cab-9ad8-96ed55e6aaf8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/29be5b1f-f84f-4cab-9ad8-96ed55e6aaf8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e97dd2d5-b8ce-4a6e-949c-833df6a8a823"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler untersuchen sprachliche Verständigung und sprachliche Variation in Texten, Gesprächen und Formen digitaler Kommunikation und beschreiben Unterschiede zwischen Mündlichkeit und Schriftlichkeit. Sie nutzen ihre Einsichten für das eigene Sprachhandeln.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/22c7b269-b3ef-4dd6-8397-ad9d6fc12014"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/54459a32-1634-47bf-8a7e-2b14e2c94a92"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6"/>
        <skos:notation>5.1</skos:notation>
        <skos:prefLabel xml:lang="de">Sprachliche Verständigung und sprachliche Vielfalt untersuchen </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/29d2f2e4-9e70-4db3-9abc-0ec65ffdec63 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/29d2f2e4-9e70-4db3-9abc-0ec65ffdec63">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">sammeln sprachliche und gestalterische Mittel: Wörter und Wortfelder, Formulierungen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/3a036be4-0eaf-4818-899d-28c6ea24b062 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/3a036be4-0eaf-4818-899d-28c6ea24b062">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.10</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen handlungs- und produktionsorientierte Verfahren, um ihre Deutung zu entwickeln und darzustellen und um Strukturen literarischer Texte nachzuvollziehen (z. B. innerer Monolog, Standbild, Parallelgedicht; Clip, Storyboard/ Fotostory),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/3a70fbfb-85d1-4a92-a0d0-1bfda5ab7ae1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/3a70fbfb-85d1-4a92-a0d0-1bfda5ab7ae1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">vergleichen an ausgewählten Beispielen sprachliche Phänomene in verschiedenen Sprachen (u. a. Herkunftssprachen, Fremdsprachen), auch im Hinblick auf individuelle und gesellschaftliche Mehrsprachigkeit.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/3c342f81-e94b-4ede-8c0c-b30c51ee1388 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/3c342f81-e94b-4ede-8c0c-b30c51ee1388">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.4</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen unterschiedliche Schreibformen: kooperative (z. B. Schreibkonferenzen), produktive (z. B. Umschreiben, Weiterschreiben) und kreative (z. B. Schreiben zu Bildern),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/3f0630d4-bdb8-4ec9-83bd-cb4f1fbc70b2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/3f0630d4-bdb8-4ec9-83bd-cb4f1fbc70b2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">ermitteln an ausgewählten Beispielen den funktionalen Beitrag von Wörtern verschiedener Wortarten zum Aufbau von Sätzen, </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/31b113f5-60d9-4eb8-80ec-ad1d79cef4a1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/31b113f5-60d9-4eb8-80ec-ad1d79cef4a1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/161fd7aa-41d2-4c28-b43a-aee961435408"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">beschreiben und vergleichen den Aufbau von Texten und Gesprächen strukturell und funktional unter Anwendung von Fachbegriffen (s. Übersicht: Grundlegende sprachliche Struktureinheiten) und ermitteln Gemeinsamkeiten und Unterschiede,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/31f51aec-df13-4df3-b1f0-4477e6b5697c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/31f51aec-df13-4df3-b1f0-4477e6b5697c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden angeleitet und an ausgewählten Beispielen Ausprägungen von Sprache und Sprachvariation (z. B. Standardsprache – Alltagssprache, Fachsprache, Dialekt, Regionalsprache; Mehrsprachigkeit; Sprache in der digitalen Kommunikation; Deutsche Gebärdensprache) und reflektieren Verwendungsweisen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/313db9bf-4b46-45dc-9a00-e1af3f43a23b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/313db9bf-4b46-45dc-9a00-e1af3f43a23b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">die Aufgabenstellung (Schreibsituation, Schreibanlass, Textfunktion und Textsorte) Aufbau, Inhalt und Formulierungen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/3327c908-bd5c-4458-8e7a-091c6297fdf6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/3327c908-bd5c-4458-8e7a-091c6297fdf6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e2779e2c-fc84-4a72-844f-f105d37ddf89"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/fef80bf6-6888-4095-98de-9512b1e42dac"/>
        <skos:notation>5.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">Rechtschreibung</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/34734e40-0e14-407c-9557-d6703a04908d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/34734e40-0e14-407c-9557-d6703a04908d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b5604218-e8e1-4d64-af8b-06e1b1dc7049"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.2.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">Verstehen und Nicht-Verstehen zum Ausdruck bringen, z. B. durch Nachfragen, durch Kommentare, </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/35bc9b6e-0ac7-4b94-9a23-1952a225c9d3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/35bc9b6e-0ac7-4b94-9a23-1952a225c9d3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c5276f76-7440-496d-a034-17301994bc9f"/>
        <skos:definition xml:lang="de">(Formate: z. B. Chats, Videoclips, Computerspiele, Podcasts; Umgebungen: z. B. soziale Netzwerke, Online-Portale, Lernplattformen, Suchmaschinen)</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/ce6e22de-757f-458a-ad13-8cfc9df0d1c0"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d9babf0c-7822-4da9-9772-effdfc564454"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/ddb62fc0-70e7-4474-830c-4a24c1a939b6"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2c7ee16e-2da3-4892-99ed-9c56992fc3d3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38d7ce27-1508-4244-94dc-087874acb0bb"/>
        <skos:notation>4.6</skos:notation>
        <skos:prefLabel xml:lang="de">Digitale Formate und Umgebungen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/36c643df-74d1-4e85-ac75-2895fb4777a3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/36c643df-74d1-4e85-ac75-2895fb4777a3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d5558e46-feb6-4a89-ba94-75b51b546baa"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">erschließen aus einem oder mehreren klar strukturierten Texten zielgerichtet relevante Informationen, ordnen, selegieren und vergleichen diese und prüfen ihren Sachgehalt,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/366b4de3-aaec-427b-b3f9-805ee5143ec5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/366b4de3-aaec-427b-b3f9-805ee5143ec5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/54459a32-1634-47bf-8a7e-2b14e2c94a92"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">kennen Grundbedingungen gelingender Kommunikation und unterscheiden die Handlungs-, Inhalts- und Beziehungsebene,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/37dc681d-5dd9-45bb-9e0e-0df84bb33674 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/37dc681d-5dd9-45bb-9e0e-0df84bb33674">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.4.6</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Informationsquellen gezielt und angemessen, insbesondere Bibliotheken, Nachschlagewerke, Zeitungen, Newsportale, digitale Suchdienste und (digitale) Ressourcen zur deutschen Sprache,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b912ae89-9eff-46d6-bea8-d372ae33bc01"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler beherrschen grundlegende Schreibfunktionen, sie können Texte zu unterschiedlichen Textsorten und Funktionen für sich, für auch unbekannte und unterschiedliche Adressaten verständlich und angemessen formulieren (selbstständig und kooperativ). Sie nutzen digitale Schreibumgebungen und wenden Schreibstrategien an.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b40314f0-b078-4b3b-b52a-05f733eb4311"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f84c0c30-e889-4f14-be60-1363f756deb0"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0375eafd-ab6a-4028-9f75-acd017af63d1"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/122e73a2-7114-4c66-ab72-d91310970967"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/16f5c5a3-e5db-4b96-bc1b-dd2f884e8e32"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/3c342f81-e94b-4ede-8c0c-b30c51ee1388"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/52eb0d23-cd08-4667-89f8-c4bd3b6f7b59"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/55e05f94-2cde-4c85-ba40-9547bfc5c294"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58cbd646-d58c-417b-8cfa-17cb61e01b24"/>
        <skos:notation>2.5</skos:notation>
        <skos:prefLabel xml:lang="de">Texte formulieren</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/38d7ce27-1508-4244-94dc-087874acb0bb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/38d7ce27-1508-4244-94dc-087874acb0bb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/35bc9b6e-0ac7-4b94-9a23-1952a225c9d3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.6.4</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen am Beispiel den Zusammenhang von digitalen Nutzerprofilen, Algorithmen, Gestaltung und Wirkung digitaler Angebote und bewerten ihn,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/39f03fee-4419-43a7-a9b4-312509623896 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/39f03fee-4419-43a7-a9b4-312509623896">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b5604218-e8e1-4d64-af8b-06e1b1dc7049"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.2.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Gesprächsprozesse verfolgen, verstehen und Inhalte für die Kommunikation nutzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/4d84ead8-2aba-4f85-ba3d-81abfb43fbdf -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/4d84ead8-2aba-4f85-ba3d-81abfb43fbdf">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b912ae89-9eff-46d6-bea8-d372ae33bc01"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler nutzen Kriterien für die Textüberprüfung und ‐überarbeitung. Im Rahmen von Feedbackprozessen identifizieren sie den Überarbeitungsbedarf eigener und fremder Texte. Sie können Texte insbesondere hinsichtlich ihrer Angemessenheit und Verständlichkeit inhaltlich, sprachlich und auf Textkohärenz bezogen überarbeiten und dabei Überarbeitungsstrategien reflektiert anwenden.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/ddb5279f-186f-4690-817f-3e34ba81d40f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e7e6ceff-6404-41db-b5a4-44de7611ec6c"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2c39e20f-9eef-4c55-9cf3-d2993abede9a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/4eccfd15-43c6-458c-9dc9-1d418d198d59"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/89e0f3e4-8133-4cc5-a3e6-8548a291134d"/>
        <skos:notation>2.6</skos:notation>
        <skos:prefLabel xml:lang="de">Texte überarbeiten</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/4e723264-3c2e-4b28-bdc6-42dc40a400f9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/4e723264-3c2e-4b28-bdc6-42dc40a400f9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen ihre Vorbereitungen (z. B. Notizen) für die sach- und situationsgerechte Gestaltung vertrauter und unvertrauter Situationen, z. B. Bewerbungsgespräch, Diskussion, Präsentation,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/4eccfd15-43c6-458c-9dc9-1d418d198d59 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/4eccfd15-43c6-458c-9dc9-1d418d198d59">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/4d84ead8-2aba-4f85-ba3d-81abfb43fbdf"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.1</skos:notation>
        <skos:prefLabel xml:lang="de">erkennen anhand vorgegebener Kriterienkataloge den Überarbeitungsbedarf von Texten, überlegen Schritte zur Textüberarbeitung und setzen diese um, </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/402ce841-8da6-473d-8354-afb14b2432d5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/402ce841-8da6-473d-8354-afb14b2432d5">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8d07cde9-e806-410d-832e-792b1a55fe25"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/be4232cf-23ab-4475-88a8-0d636241ddf6"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d322cc0b-366b-4fa2-a1df-0c14e724b2e6"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/52f8db60-09f3-4ad6-9c0f-d5af81169361"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/9977f7ba-efc0-4e8a-90e5-16c638107ae5"/>
        <skos:notation>1.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">hören in vertrauten und unvertrauten Situationen sinnverstehend zu,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/46e49922-b3c3-4075-8a39-03cc89a15e72 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/46e49922-b3c3-4075-8a39-03cc89a15e72">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:definition xml:lang="de">Die gesprochene Sprache ist ein zentrales Mittel schulischer und außerschulischer Kommunikation. Dabei ist gesprochene Sprache immer auch soziales Handeln. Die Schülerinnen und Schüler lernen, wichtige kommunikative Situationen in persönlichen, schulischen und öffentlichen Zusammenhängen angemessen und adressatengerecht zu bewältigen. Dabei nutzen sie sprachliche Werkzeuge auch im digitalen Kontext. Die Schülerinnen und Schüler verfügen über eine demokratische Gesprächskultur, die von aufmerksamem Zuhören und respektvollem Gesprächsverhalten geprägt ist. Sie nutzen die Standardsprache. Sie achten auf gelingende Kommunikation und damit auch auf die Wirkung ihres sprachlichen Handelns. Sie führen und moderieren Gespräche und realisieren kommunikative Funktionen wie erzählen, informieren und argumentieren. Sie diskutieren, gestalten ihr Sprechen bewusst und leisten konstruktive gesprochene Beiträge zum Unterricht. Im Kompetenzbereich „Sprechen und Zuhören“ sind die kommunikativen Anforderungen von besonderer Bedeutung. In den Kernbereichen Zu anderen sprechen und Verstehend zuhören sind deswegen grundlegende und situationsübergreifende Standards des Vor anderen Sprechens und Mit anderen Sprechens dargestellt. Der Kernbereich Mit anderen sprechen bezieht sich dementsprechend durchweg auf Situationen, in denen Schülerinnen und Schüler aufeinander Bezug nehmen. Der Kernbereich Vor anderen sprechen umfasst Standards zu solchen Situationen, in denen die Schülerinnen und Schüler eine Zeit lang das Rederecht wahrnehmen, z. B. Präsentationen, Reden. Die Beiträge können dabei jeweils vorbereitet oder unvorbereitet sein. In kommunikativen Situationen können die Schülerinnen und Schüler mit Blick auf die kommunikativen Ziele und die Gesprächspartnerinnen und -partner bewusst das Register wechseln, z. B. Standardsprache, Alltagssprache, Bildungssprache, Fachsprache.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8d07cde9-e806-410d-832e-792b1a55fe25"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79"/>
        <skos:notation>1</skos:notation>
        <skos:prefLabel xml:lang="de">Sprechen und Zuhören</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/482365ed-8a23-4c11-80dd-777d4b8aade4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/482365ed-8a23-4c11-80dd-777d4b8aade4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/21b9e2a9-e0f5-463d-8ace-af56ac97f58b"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.2.5</skos:notation>
        <skos:prefLabel xml:lang="de">kennen und nutzen die Interpunktionszeichen: Punkt, Komma, Fragezeichen, Ausrufezeichen, Anführungszeichen, Semikolon, Doppelpunkt, Auslassungspunkte, Gedankenstrich, Klammern,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/486af035-8c97-4b17-a658-c9c96711f626 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/486af035-8c97-4b17-a658-c9c96711f626">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.5</skos:notation>
        <skos:prefLabel xml:lang="de">vergleichen Texte in Hinblick auf Themen und Gestaltung und nehmen begründete Wertungen vor (auch intermedial, z. B. Buch und Film),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/49c649ab-fd6d-4058-8958-24132ebaa4f7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/49c649ab-fd6d-4058-8958-24132ebaa4f7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">Kohärenz und Verständlichkeit,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e97dd2d5-b8ce-4a6e-949c-833df6a8a823"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können an ausgewählten Beispielen sprachliche Strukturen in Texten und Gesprächen, in gesprochener und geschriebener Sprache – auch unter Nutzung von Ressourcen zur deutschen Sprache (z. B. Wörterbücher, Grammatiken) – beschreiben und nutzen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/db83727a-5a4b-438f-8bae-ec2e6166ab01"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/161fd7aa-41d2-4c28-b43a-aee961435408"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/23e368b0-9822-445d-98da-473f9edc37c5"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/3327c908-bd5c-4458-8e7a-091c6297fdf6"/>
        <skos:notation>5.2</skos:notation>
        <skos:prefLabel xml:lang="de">Sprachliche Strukturen untersuchen und nutzen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5caa8223-ddcd-498f-b702-708f9428a6b7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5caa8223-ddcd-498f-b702-708f9428a6b7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.3.7</skos:notation>
        <skos:prefLabel xml:lang="de">sprechen über das sprachliche und literarische Lernen sowie über Lernerfahrungen: geben Beobachtungen wieder, beschreiben Sachverhalte, geben Erklärungen und Begründungen, reflektieren.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5ddc5b5a-4fa3-45f8-b340-dd18b3b275b0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5ddc5b5a-4fa3-45f8-b340-dd18b3b275b0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a96afc32-a9c3-485a-9225-7b300b8028a4"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">erläutern,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5ef786f2-8308-4151-b5de-daa241ff7dfb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5ef786f2-8308-4151-b5de-daa241ff7dfb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/22c7b269-b3ef-4dd6-8397-ad9d6fc12014"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen die Anforderungen an die Rollen von Sprecher/Schreiber – Hörer/Leser im Hinblick auf gelingende Kommunikation und werden diesen im eigenen sprachlichen Handeln gerecht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5fedea0e-1e34-4b19-820a-5a9e14344980 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5fedea0e-1e34-4b19-820a-5a9e14344980">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f3136ad6-8044-4ec4-8ca8-b03ed674f368"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>3.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">orientieren sich in und zwischen Texten unterschiedlicher Komplexität und verschaffen sich auch mithilfe textueller Lesehilfen (u. a. Überschriften, Zwischenüberschriften, Absätze, Abbildungen, Illustrationen) einen Überblick: überfliegendes, selegierendes, navigierendes Lesen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc">
