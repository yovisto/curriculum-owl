        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e65ed8c4-0160-406b-b2bb-4c2994af4a6a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f07f0ba9-b5b1-4d78-a446-23375f96f398"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/13afc739-bf35-415c-a36a-4be460c45b4d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/24da74be-a6d2-4b8f-8e73-d5e1c3016dc2"/>
