        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.2.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">beurteilen Aussagen zum Gehörten begründet,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/54459a32-1634-47bf-8a7e-2b14e2c94a92 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/54459a32-1634-47bf-8a7e-2b14e2c94a92">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/29be5b1f-f84f-4cab-9ad8-96ed55e6aaf8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c0935610-c645-4d19-909c-f43d60383e84"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d03380b1-5915-4059-b212-de73f3aa509a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/366b4de3-aaec-427b-b3f9-805ee5143ec5"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7f50ad0b-c68e-4607-9e7c-2a035ad71382"/>
        <skos:notation>5.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Sprachliche Verständigung als Form des Handelns</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/55e05f94-2cde-4c85-ba40-9547bfc5c294 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/55e05f94-2cde-4c85-ba40-9547bfc5c294">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.8</skos:notation>
        <skos:prefLabel xml:lang="de">formulieren eigene Positionen und begründen diese</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/5701bce3-62b9-459e-8497-82d0e7b3f4d8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5701bce3-62b9-459e-8497-82d0e7b3f4d8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/22c7b269-b3ef-4dd6-8397-ad9d6fc12014"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen monologische (z. B. Texte, Podcasts) und interaktionale Formen des Sprechens und des Schreibens (z. B. Gespräche, Chats) hinsichtlich ihrer Struktur und Organisation (ausgewählte Aspekte),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/571bdb3d-5715-4f41-a25c-d5f740556a57 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/571bdb3d-5715-4f41-a25c-d5f740556a57">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.4.4</skos:notation>
        <skos:prefLabel xml:lang="de">planen und gestalten Formen des szenischen Sprechens und Spielens, z. B. szenische Lesung, Szene, Aufführung, Hörspiel, Videoclip, Standbild, Improvisation.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/574a72fe-53f3-47bc-8787-771c3e980def -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/574a72fe-53f3-47bc-8787-771c3e980def">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b912ae89-9eff-46d6-bea8-d372ae33bc01"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler beachten bei der Planung und Strukturierung, beim Formulieren und beim Überarbeiten von Texten allgemeine Textmerkmale, Adressaten, den Schreibanlass, Schreibfunktion und Textsorte, Formen des Schreibens und den Einsatz digitaler Schreibwerkzeuge, den Umgang mit Quellen beim Belegen und Zitieren und die Anwendung von Schreibstrategien.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.3</skos:notation>
        <skos:prefLabel xml:lang="de">Texte verfassen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/58cbd646-d58c-417b-8cfa-17cb61e01b24 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/58cbd646-d58c-417b-8cfa-17cb61e01b24">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.9</skos:notation>
        <skos:prefLabel xml:lang="de">integrieren Textbelege und andere Quellen formal richtig in den eigenen Text (zitieren),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/29be5b1f-f84f-4cab-9ad8-96ed55e6aaf8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0509e872-a2c5-40e7-b0dc-2ca26f05ca49"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/3a70fbfb-85d1-4a92-a0d0-1bfda5ab7ae1"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/31f51aec-df13-4df3-b1f0-4477e6b5697c"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7117c7b1-6fbe-4a9b-9ec4-0fb71e7733fb"/>
        <skos:notation>5.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">Ausprägungen und Bedingungen sprachlicher Variation und Vielfalt</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6a351609-53af-45aa-8fd9-f5287d31544c -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6a351609-53af-45aa-8fd9-f5287d31544c">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/161fd7aa-41d2-4c28-b43a-aee961435408"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen ihr Wissen zu sprachlichen Gestaltungsmitteln für die Beschreibung und Untersuchung von (auch literarischen) Texten, Reden, Gesprächen und Formen digitaler Kommunikation.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6b6818ff-2fb9-4f82-8803-f00cecd26ef4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6b6818ff-2fb9-4f82-8803-f00cecd26ef4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8d07cde9-e806-410d-832e-792b1a55fe25"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.2.3</skos:notation>
        <skos:prefLabel xml:lang="de">wählen aus grundlegenden kognitiven und metakognitiven Strategien aus, die sie vor, während und nach dem Zuhören einsetzen, z. B. die Erwartungshaltung zielorientiert steuern, Aufmerksamkeit steuern, Informationen sichern und zusammenfassen: Notizen, Protokoll.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6c87ea0f-f367-4e5a-b5fc-b3be7d4ddd77 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6c87ea0f-f367-4e5a-b5fc-b3be7d4ddd77">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a96afc32-a9c3-485a-9225-7b300b8028a4"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.3.6</skos:notation>
        <skos:prefLabel xml:lang="de">appellieren,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6f7a58e5-03e7-4ec2-95b9-9ca817cf1a9e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6f7a58e5-03e7-4ec2-95b9-9ca817cf1a9e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.4.2</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen typische Werkzeuge für das Sprechen vor anderen, z. B. Tafel, Plakat, Moderationskarten, Präsentationssoftware, Whiteboard,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6017320d-36a0-4ef9-9648-e048ba82ffdf -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6017320d-36a0-4ef9-9648-e048ba82ffdf">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler verfügen über kognitive und metakognitive Lesestrategien zum Einsatz vor dem Lesen, während des Lesens und nach dem Lesen. Sie können die Strategien beim Lesen eines unbekannten oder schwierigen Textes oder beim vergleichenden Lesen mehrerer Texte den Anforderungen der Texte und dem Leseziel entsprechend auswählen und ausführen. Sie reflektieren sowohl ihr Verstehen als auch ihren Einsatz von Lesestrategien.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/de51f849-0e82-4002-871c-1dc9e81c5aab"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2d5e280d-6627-4036-909e-636a8515cf21"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/93637534-243a-42d3-9251-6d6ffd90a7f7"/>
        <skos:notation>3.4</skos:notation>
        <skos:prefLabel xml:lang="de">Lesefähigkeiten: Über Strategien zum Leseverstehen verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/60359b06-253d-4105-9241-ee1c3772c840 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/60359b06-253d-4105-9241-ee1c3772c840">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.5</skos:notation>
        <skos:prefLabel xml:lang="de">bauen Redebeiträge in vertrauten und unvertrauten Situationen für andere nachvollziehbar und sinnvoll auf,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6140d957-476e-4bc8-a2f1-981007595af9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6140d957-476e-4bc8-a2f1-981007595af9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>3.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">ermitteln explizite oder durch einfaches Schließen zu gewinnende Textinformationen auf lokaler Ebene und beachten dabei auch sprachliche Mittel zur Sicherung des Textzusammenhangs,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/4d84ead8-2aba-4f85-ba3d-81abfb43fbdf"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/187942fd-28f7-4e90-96dd-b333ff5d0d6b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/313db9bf-4b46-45dc-9a00-e1af3f43a23b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/49c649ab-fd6d-4058-8958-24132ebaa4f7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6964703b-4791-43d3-a24f-5beae2d3b15a"/>
        <skos:notation>2.6.3</skos:notation>
        <skos:prefLabel xml:lang="de">überprüfen eigene und fremde Texte kriteriengeleitet auf der Basis von Rückmeldungen und überarbeiten diese (auch kooperativ) im Hinblick auf:</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/648c177c-9c53-40f1-9efb-5c17cdb5daf0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/648c177c-9c53-40f1-9efb-5c17cdb5daf0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.4.4</skos:notation>
        <skos:prefLabel xml:lang="de">klären entsprechend dem Schreibziel die Schreibabsicht, Schreibsituation und Textfunktion,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/65013396-a4c7-4246-9357-ff8596f41b1a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/65013396-a4c7-4246-9357-ff8596f41b1a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.3.5</skos:notation>
        <skos:prefLabel xml:lang="de">praktizieren ausgewählte Gesprächs- und Lernformen, z. B. literarische Gespräche, Rollenspiele, Vorstellungsgespräche, Diskussionen, videogestützte Gespräche, Interviews,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/65511534-3b94-4a37-8931-0d6840f5989d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/65511534-3b94-4a37-8931-0d6840f5989d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/837374d8-d20c-4c8c-9469-10239cb22b79"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">leisten längere, auch freie Beiträge adressatenorientiert und mit Hilfe eines Stichwortzettels/einer Gliederung/von Notizen/eines Textes, z. B. Referat, Präsentation, Rede, Gedicht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/660f9f0b-81aa-48cc-b81f-5f178eb6ac93 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/660f9f0b-81aa-48cc-b81f-5f178eb6ac93">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.4</skos:notation>
        <skos:prefLabel xml:lang="de">formulieren Wertungen von Texten und begründen sie,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/68660d81-9d90-42ee-a2e7-bdb1764d4686 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/68660d81-9d90-42ee-a2e7-bdb1764d4686">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/21b9e2a9-e0f5-463d-8ace-af56ac97f58b"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a40eff8f-ad77-4a60-b16b-db9ac9fbe1d6"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b5ebaf04-23bd-4af0-a3e9-aa168df7f904"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2f86e89e-41de-4e6a-9939-61a99b8ab9a8"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8abc2d4a-1a9d-4b50-913c-6becb4cff913"/>
        <skos:notation>2.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">kennen wort- und satzbezogene Rechtschreibstrategien und wenden diese an</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/6964703b-4791-43d3-a24f-5beae2d3b15a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/6964703b-4791-43d3-a24f-5beae2d3b15a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Angemessenheit sprachlicher Gestaltungsmittel, Rechtschreibung und Grammatik,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/46e49922-b3c3-4075-8a39-03cc89a15e72"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können in vertrauten und unvertrauten Situationen mit mehreren Teilnehmerinnen und Teilnehmern aufeinander Bezug nehmen, um kommunikative Ziele zu verfolgen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/ec20ea82-620a-4728-bbba-2384c849c045"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f0e7b014-6514-449b-b61f-b279e68bbf95"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/10095a51-967e-4876-8ee9-32eb708abe6e"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5caa8223-ddcd-498f-b702-708f9428a6b7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/65013396-a4c7-4246-9357-ff8596f41b1a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8197e8bf-54d8-4e84-bbd2-2e94e7ddc149"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/894965fa-8f06-4250-9714-18275aa61669"/>
        <skos:notation>1.3</skos:notation>
        <skos:prefLabel xml:lang="de">Mit anderen sprechen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/7f50ad0b-c68e-4607-9e7c-2a035ad71382 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/7f50ad0b-c68e-4607-9e7c-2a035ad71382">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/54459a32-1634-47bf-8a7e-2b14e2c94a92"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen an ausgewählten Beispielen Normen demokratischer Gesprächs- und Kommunikationskultur und sprachlicher Höflichkeit (auch in digitalen Umgebungen) und nutzen diese für die sprachliche Gestaltung eigener Äußerungen sowie für den Umgang mit den sprachlichen Äußerungen anderer,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/70d79fbd-d094-4aa7-99da-377c830b1cd3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/70d79fbd-d094-4aa7-99da-377c830b1cd3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.4.8</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Vorbereitung der Textproduktion, z. B. Anfertigen von Notizen und Skizzen, Anlegen von Stoffsammlungen, Exzerpieren aus digitalen Informationsressourcen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/7117c7b1-6fbe-4a9b-9ec4-0fb71e7733fb -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/7117c7b1-6fbe-4a9b-9ec4-0fb71e7733fb">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden informelle und elaborierte Mündlichkeit/Schriftlichkeit und können an konkreten Beispielen Wirkungen einschätzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/714814ab-2e62-4c54-8a78-cfb70818bd6b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/714814ab-2e62-4c54-8a78-cfb70818bd6b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Wörter, Wendungen und Formulierungsmuster funktional und erweitern ihren Wortschatz,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/720f3b4f-a62a-40be-96e8-314ed3819cd7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/720f3b4f-a62a-40be-96e8-314ed3819cd7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b40314f0-b078-4b3b-b52a-05f733eb4311"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">Darstellen: Informieren auf Basis klar strukturierter Material- und Textquellen (informierend materialgestützt schreiben)</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/72805a24-fe05-4adb-a722-45ed5be23775 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/72805a24-fe05-4adb-a722-45ed5be23775">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c4e6462b-0905-4af1-bc19-c9f3be62e835"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.1.8</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen Angaben zum Wirklichkeitsbezug von Texten als Rezeptionshinweise (z. B. Gattungs-/Genrebezeichnungen: Roman, Satire, Krimi, Biopic, Dokumentation, Doku-Drama),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/734f2b48-e4ae-4375-b060-21cf29a4d5f1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/734f2b48-e4ae-4375-b060-21cf29a4d5f1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler lesen leise und laut, automatisiert, genau, sinngestaltend und zügig.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a6405631-c41c-49cd-b9a8-6bbf7da3fe9f"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/cb5aca95-b389-4335-b99c-c0d8ecff93d2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e4f81ed1-98d2-4cd7-8923-b1681f78d8e3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/995a361a-3523-4a89-b7d8-5d1cd2d73129"/>
        <skos:notation>3.1</skos:notation>
        <skos:prefLabel xml:lang="de">Lesefertigkeit: Flüssig lesen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/46e49922-b3c3-4075-8a39-03cc89a15e72"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler können in vertrauten und unvertrauten Situationen des vor und mit anderen Sprechens angemessen und adressatengerecht kommunizieren, z. B. in Gesprächen, bei Präsentationen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a96afc32-a9c3-485a-9225-7b300b8028a4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/ad390051-6481-42c3-a585-b9ba73ff9d95"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d9b92698-9e6e-4894-b9e8-bfc69d8f4685"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f0f4294d-68d6-4f1c-937e-2bcbbe2fdf32"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/fb1c8c4b-61c9-4f50-b68b-0222c626af46"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/00cc868e-cbe4-4f14-ae87-c391c4f393f0"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/1a777240-6fb6-4834-934e-162c86d6dcd7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/4e723264-3c2e-4b28-bdc6-42dc40a400f9"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/60359b06-253d-4105-9241-ee1c3772c840"/>
        <skos:notation>1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Zu anderen sprechen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/759d5a59-947f-436e-b940-50da863e7cb6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/759d5a59-947f-436e-b940-50da863e7cb6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/21b9e2a9-e0f5-463d-8ace-af56ac97f58b"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben Kernwörter richtig (Inhalts- und Funktionswörter)</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/76dcc44f-1a27-4991-bb4f-1dcd5bf52421 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/76dcc44f-1a27-4991-bb4f-1dcd5bf52421">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen grammatische Operationen und Fachbegriffe für die Untersuchung sprachlicher Struktureinheiten (s. Übersicht: Verzeichnis grundlegender grammatischer Fachausdrücke),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/780818e3-1245-4ded-a298-ef2aeff075ab -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/780818e3-1245-4ded-a298-ef2aeff075ab">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/27f070fa-bb81-4a63-b008-fb9e532d3deb"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">bewerten Rechercheergebnisse (insbes. Internet) im Hinblick auf ihre thematische Relevanz und Verlässlichkeit (Informationsangebote vergleichen, Quellenangaben und -qualität prüfen, Informationen zu Autorinnen und Autoren recherchieren, mögliche Textfunktionen beachten).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8abc2d4a-1a9d-4b50-913c-6becb4cff913 -->

