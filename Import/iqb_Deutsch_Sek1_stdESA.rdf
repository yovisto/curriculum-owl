    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/9977f7ba-efc0-4e8a-90e5-16c638107ae5 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/9977f7ba-efc0-4e8a-90e5-16c638107ae5">
