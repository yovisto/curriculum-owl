        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">gestalten Texte (handschriftlich und mithilfe digitaler Schreibwerkzeuge) übersichtlich und zielorientiert, bauen sie sinnvoll auf und strukturieren diese: z. B. Blattaufteilung, Seitenränder, Absatzstrukturen, Textformatierung und Layout.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8105e21f-5abe-405a-9e59-9eaddfdbb1ba -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/8105e21f-5abe-405a-9e59-9eaddfdbb1ba">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
