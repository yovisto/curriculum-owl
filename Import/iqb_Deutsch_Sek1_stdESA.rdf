        <skos:notation>4.4</skos:notation>
        <skos:prefLabel xml:lang="de">Literatur in unterschiedlicher Medialität</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f3136ad6-8044-4ec4-8ca8-b03ed674f368 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f3136ad6-8044-4ec4-8ca8-b03ed674f368">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler verfügen über Techniken zur Orientierung in und zwischen Texten.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5fedea0e-1e34-4b19-820a-5a9e14344980"/>
        <skos:notation>3.2</skos:notation>
        <skos:prefLabel xml:lang="de">Lesefertigkeiten: Über Lesetechniken verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f48bb92c-661d-442a-aa11-5a1e7e31aa97 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f48bb92c-661d-442a-aa11-5a1e7e31aa97">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.1.4</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen an ausgewählten Beispielen Satzstrukturen, insbesondere: Prädikat, Satzglieder, Attribute, Satz- und Nebensatztypen (z. B. Satzklammer, Felderstruktur),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f4afcfa6-af16-49c2-b2dc-4099e8374cf1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f4afcfa6-af16-49c2-b2dc-4099e8374cf1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b40314f0-b078-4b3b-b52a-05f733eb4311"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">Darstellen von Textbearbeitungen: Inhaltsangabe, Ergebnisdarstellung einer Textuntersuchung entsprechend den Anforderungen des Erschließens von Texten und Medien</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f84c0c30-e889-4f14-be60-1363f756deb0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f84c0c30-e889-4f14-be60-1363f756deb0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.1</skos:notation>
        <skos:prefLabel xml:lang="de">formulieren Texte gedanklich geordnet, verständlich, kohärenzstiftend und strukturiert sowie sprachlich angemessen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f90dc6f7-e8cf-4699-8910-33d5d90ee554 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f90dc6f7-e8cf-4699-8910-33d5d90ee554">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/a96afc32-a9c3-485a-9225-7b300b8028a4"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">informieren: berichten, beschreiben,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/fb1c8c4b-61c9-4f50-b68b-0222c626af46 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/fb1c8c4b-61c9-4f50-b68b-0222c626af46">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.6</skos:notation>
        <skos:prefLabel xml:lang="de">berücksichtigen typische Wirkungen der Redeweise situations- und adressatengerecht: Lautstärke, Betonung, Sprechtempo, Pausen, Körpersprache (Gestik/Mimik/Körperhaltung),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/fef80bf6-6888-4095-98de-9512b1e42dac -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/fef80bf6-6888-4095-98de-9512b1e42dac">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/3327c908-bd5c-4458-8e7a-091c6297fdf6"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen Schreibweise von Wörtern, Wortgruppen und Sätzen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/0bc7d2e0-d0a1-44c5-bdf8-11ac3d06153d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/0bc7d2e0-d0a1-44c5-bdf8-11ac3d06153d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>3.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">verknüpfen Textinformationen, ziehen auch unter Nutzung ihres Vorwissens Schlussfolgerungen und konstruieren ein Gesamtverständnis.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/0dd90e69-c705-4c72-a4ec-2b314f6986db -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/0dd90e69-c705-4c72-a4ec-2b314f6986db">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c4e6462b-0905-4af1-bc19-c9f3be62e835"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.1.7</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden pragmatische Textsorten und deren zentrale Funktionen (u. a. Bericht, Nachricht, Feature, Reportage [informierend]; Anleitung, Erklärvideo [instruierend]; Schulordnung [regulierend]; Kommentar, Buchkritik [überzeugend, argumentierend]),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/00cc868e-cbe4-4f14-ae87-c391c4f393f0 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/00cc868e-cbe4-4f14-ae87-c391c4f393f0">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.8</skos:notation>
        <skos:prefLabel xml:lang="de">geben in vertrauten und unvertrauten Situationen nach vorgegebenen Kriterien gezielt Feedback zu Redebeiträgen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/02c90fb5-5bb0-4afb-84ea-2627759b8b69 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/02c90fb5-5bb0-4afb-84ea-2627759b8b69">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d5558e46-feb6-4a89-ba94-75b51b546baa"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.5.7</skos:notation>
        <skos:prefLabel xml:lang="de">ermitteln an ausgewählten Beispielen die Wirkungsabsichten von Texten und beurteilen diese.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/0280da36-3f78-4d9a-9dd6-6bf2758a9151">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c3dad891-691f-4864-8bbf-9ad52689cc01"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f48bb92c-661d-442a-aa11-5a1e7e31aa97"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/3f0630d4-bdb8-4ec9-83bd-cb4f1fbc70b2"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/714814ab-2e62-4c54-8a78-cfb70818bd6b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/76dcc44f-1a27-4991-bb4f-1dcd5bf52421"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8105e21f-5abe-405a-9e59-9eaddfdbb1ba"/>
        <skos:notation>5.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Wörter und Sätze</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/0375eafd-ab6a-4028-9f75-acd017af63d1 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/0375eafd-ab6a-4028-9f75-acd017af63d1">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.5</skos:notation>
        <skos:prefLabel xml:lang="de">kennen und nutzen die Möglichkeiten digitaler Schreibwerkzeuge zur Formulierung und (formalen) Gestaltung von Texten, z. B. Schrift‐ und Textformatierung, Arrangieren und Re‐Arrangieren von Textbausteinen und Medienobjekten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/0509e872-a2c5-40e7-b0dc-2ca26f05ca49 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/0509e872-a2c5-40e7-b0dc-2ca26f05ca49">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/58035696-91f5-4b10-a524-bfc474d06fd6"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.1.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen ausgewählte Erscheinungen des Sprachwandels (z. B. Bedeutungswandel, Entlehnungen aus anderen Sprachen, Sprachwandel durch digitale Kommunikation),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/1a777240-6fb6-4834-934e-162c86d6dcd7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/1a777240-6fb6-4834-934e-162c86d6dcd7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.7</skos:notation>
        <skos:prefLabel xml:lang="de">wählen geeignete (multimodale) Medien mit Unterstützung sinnvoll aus und setzen diese gezielt ein, um den Inhalt und die Wirkung eigener Sprechbeiträge zu unterstützen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/1cc6fdb3-c357-4ebe-b79c-882920c0a9d8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/1cc6fdb3-c357-4ebe-b79c-882920c0a9d8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/23ff64ec-87ac-43f5-a194-896ce0d61e1e"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">schreiben Wörter, Wortgruppen und Sätze flüssig, d. h. schnell, sicher und korrekt (automatisiert), und formulieren ihre Gedanken weitgehend flüssig,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/1fb75d0a-b86b-4794-9c30-40bf9f7f4d4e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/1fb75d0a-b86b-4794-9c30-40bf9f7f4d4e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>3.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">stellen bei multimodalen Texten einfache Bezüge zwischen Text, Bild und Ton her,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/10095a51-967e-4876-8ee9-32eb708abe6e -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/10095a51-967e-4876-8ee9-32eb708abe6e">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">nehmen in Gesprächen verschiedene Rollen (z. B. Diskutantin bzw. Diskutant) ein,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:definition xml:lang="de">Lesen ist ein eigenaktiver Prozess der Sinnkonstruktion. Lesen zu können bildet sowohl die Basis dafür, eigenständig zum Vergnügen zu lesen und Leseinteresse und Lesefreude zu entwickeln, als auch dafür, aus Büchern und Texten zu lernen und sich mit ihrer Hilfe mit der Welt auseinanderzusetzen. Der Leseerwerb ist mit der Grundschulzeit nicht abgeschlossen, vielmehr müssen die Lesefertigkeiten und Lesefähigkeiten in der Sekundarstufe I weiterentwickelt und differenziert werden. Ziel ist es, dass die Schülerinnen und Schüler über grundlegende Verfahren für das Verstehen von literarischen Texten sowie Sach- und Gebrauchstexten, auch in digitaler und multimodaler Form, verfügen. Sie entnehmen selbstständig Informationen aus Texten, verknüpfen sie miteinander und verbinden sie mit ihrem Vorwissen. Dafür nutzen sie verschiedene Lesetechniken und setzen Lesestrategien gezielt ein. Die Förderung des Lesens zielt stets auch darauf, das Leseinteresse und die Lesebereitschaft der Schülerinnen und Schüler zu entwickeln und zu pflegen.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f3136ad6-8044-4ec4-8ca8-b03ed674f368"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6017320d-36a0-4ef9-9648-e048ba82ffdf"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/734f2b48-e4ae-4375-b060-21cf29a4d5f1"/>
        <skos:notation>3</skos:notation>
        <skos:prefLabel xml:lang="de">Lesen</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/122e73a2-7114-4c66-ab72-d91310970967 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/122e73a2-7114-4c66-ab72-d91310970967">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.7</skos:notation>
        <skos:prefLabel xml:lang="de">nehmen Gedanken und deutlich markierte Positionen anderer in den eigenen Text auf und positionieren sich zu ihnen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/13afc739-bf35-415c-a36a-4be460c45b4d -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/13afc739-bf35-415c-a36a-4be460c45b4d">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen zentrale Informationen zu Figuren, zu Raum- und Zeitdarstellung, Handlungs- und Konfliktverlauf sowie Atmosphäre zum Aufbau von Textverständnis und -deutung,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/16f5c5a3-e5db-4b96-bc1b-dd2f884e8e32 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/16f5c5a3-e5db-4b96-bc1b-dd2f884e8e32">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/38bf2d37-1b37-417a-900c-f8ebfe669634"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.10</skos:notation>
        <skos:prefLabel xml:lang="de">kennen Formulierungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren ihren Strategieeinsatz.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/161fd7aa-41d2-4c28-b43a-aee961435408 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/161fd7aa-41d2-4c28-b43a-aee961435408">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/31b113f5-60d9-4eb8-80ec-ad1d79cef4a1"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6a351609-53af-45aa-8fd9-f5287d31544c"/>
        <skos:notation>5.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Texte und Gespräche</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/17d39522-cc98-4cbf-b0cf-6d593caa7874 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/17d39522-cc98-4cbf-b0cf-6d593caa7874">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c4e6462b-0905-4af1-bc19-c9f3be62e835"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden epische, lyrische und dramatische Texte, insbesondere epische Kleinformen (z. B. Fabel, Kurzgeschichte), Erzählung, (Jugend-)Roman, Gedichte, Drama,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/187942fd-28f7-4e90-96dd-b333ff5d0d6b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/187942fd-28f7-4e90-96dd-b333ff5d0d6b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6158d616-bbe8-49ad-b26c-3c9ff248dad7"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.3.4</skos:notation>
        <skos:prefLabel xml:lang="de">unbekannte, unterschiedliche Adressaten,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2b5c702b-0d9c-4ea8-b436-ed92fbb5d2e7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/109af016-0593-47c2-844f-3a184cddf78a"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler lesen Texte mittlerer Komplexität sinnverstehend.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e2f12623-4fb0-4e41-a59a-be7a210bc145"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/0bc7d2e0-d0a1-44c5-bdf8-11ac3d06153d"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/1fb75d0a-b86b-4794-9c30-40bf9f7f4d4e"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6140d957-476e-4bc8-a2f1-981007595af9"/>
        <skos:notation>3.3</skos:notation>
        <skos:prefLabel xml:lang="de">Lesefähigkeiten: Leseverstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2bd9d693-6231-48b4-8df2-a4d486a91a9f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2bd9d693-6231-48b4-8df2-a4d486a91a9f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/d5558e46-feb6-4a89-ba94-75b51b546baa"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.5.5</skos:notation>
        <skos:prefLabel xml:lang="de">stellen an ausgewählten Beispielen Zusammenhänge zwischen Intentionen, Textmerkmalen, sprachlichen Gestaltungsmitteln und Wirkungen her,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2c39e20f-9eef-4c55-9cf3-d2993abede9a -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2c39e20f-9eef-4c55-9cf3-d2993abede9a">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/4d84ead8-2aba-4f85-ba3d-81abfb43fbdf"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.6.6</skos:notation>
        <skos:prefLabel xml:lang="de">kennen sowohl allgemeine als auch textsortenspezifische Überarbeitungsstrategien und wenden diese (auch kooperativ) an. Sie reflektieren sowohl ihren Überarbeitungsprozess als auch den Strategieeinsatz.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2c7ee16e-2da3-4892-99ed-9c56992fc3d3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2c7ee16e-2da3-4892-99ed-9c56992fc3d3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/35bc9b6e-0ac7-4b94-9a23-1952a225c9d3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.6.1</skos:notation>
        <skos:prefLabel xml:lang="de">kennen medienspezifische Gestaltungsmittel und beschreiben an ausgewählten Beispielen ihre Wirkung,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2d5e280d-6627-4036-909e-636a8515cf21 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2d5e280d-6627-4036-909e-636a8515cf21">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6017320d-36a0-4ef9-9648-e048ba82ffdf"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>3.4.3</skos:notation>
        <skos:prefLabel xml:lang="de">bereiten das Lesen nach (u. a. zentrale Textaussagen wiedergeben, wertende Einschätzung zum Text abgeben, anfängliche Erwartungen und Annahmen zur Textaussage prüfen und ggf. korrigieren, eigenen Wissenszuwachs prüfen).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2f86e89e-41de-4e6a-9939-61a99b8ab9a8 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2f86e89e-41de-4e6a-9939-61a99b8ab9a8">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/68660d81-9d90-42ee-a2e7-bdb1764d4686"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.2.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">prägen sich Merkwörter ein (z. B. Eigennamen, Ausnahmeschreibungen), </skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/20bc80e3-b8d6-400c-a272-a56c43306f2f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/20bc80e3-b8d6-400c-a272-a56c43306f2f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b40314f0-b078-4b3b-b52a-05f733eb4311"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.5.3.5</skos:notation>
        <skos:prefLabel xml:lang="de">Appellieren: Auffordern, Bitten, Wünschen, Begründen (Argumentieren), Erörtern, Überzeugen, z. B. in den Textsorten: Aufruf, Werbeanzeige, Stellungnahme, Antrag, Bewerbungsanschreiben, Buch-, Spiel- oder Filmkritik,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/2019e64d-e5b5-40bc-8659-3b2b306c7a67 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/2019e64d-e5b5-40bc-8659-3b2b306c7a67">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c4e6462b-0905-4af1-bc19-c9f3be62e835"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.1.6</skos:notation>
        <skos:prefLabel xml:lang="de">unterscheiden zentrale Textmuster (Erzählen, Berichten, Beschreiben, Argumentieren),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/21b9e2a9-e0f5-463d-8ace-af56ac97f58b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/21b9e2a9-e0f5-463d-8ace-af56ac97f58b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b912ae89-9eff-46d6-bea8-d372ae33bc01"/>
        <skos:definition xml:lang="de">Die Schülerinnen und Schüler schreiben häufig vorkommende Wörter wie auch häufige Fach- und Fremdwörter richtig, sie beachten die Zeichensetzung, entwickeln ihre Fehleraufmerksamkeit weiter, kennen Rechtschreibstrategien und Rechtschreibhilfen und wenden diese zur Klärung fraglicher Schreibungen an. Sie nutzen selbstständig Übungsformen und überprüfen und korrigieren Texte auf ihre orthografische Richtigkeit</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/b170a110-fe7d-4273-a139-3113cab6c17a"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c29026f8-a942-4726-890d-a952cb7bd486"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/e28e83e4-09d7-4b8c-81b7-18b912e6e764"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/482365ed-8a23-4c11-80dd-777d4b8aade4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/68660d81-9d90-42ee-a2e7-bdb1764d4686"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/759d5a59-947f-436e-b940-50da863e7cb6"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/8e58bbc2-c9dc-408a-b913-09f74c02b0bd"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/82d4b8e3-2649-4837-998b-d72ae9bfa419"/>
        <skos:notation>2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Orthografisch schreiben</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/22c7b269-b3ef-4dd6-8397-ad9d6fc12014 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/22c7b269-b3ef-4dd6-8397-ad9d6fc12014">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/29be5b1f-f84f-4cab-9ad8-96ed55e6aaf8"/>
