        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6b6818ff-2fb9-4f82-8803-f00cecd26ef4"/>
        <skos:notation>1.2</skos:notation>
        <skos:prefLabel xml:lang="de">Verstehend zuhören</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8e29b4f0-8940-47db-a63e-7f6da724956e -->

