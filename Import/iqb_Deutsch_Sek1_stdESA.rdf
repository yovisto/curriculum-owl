        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/5c52638a-8d37-429f-88ea-cbf5a98bbbce"/>
        <skos:notation>5</skos:notation>
        <skos:prefLabel xml:lang="de">Sprache und Sprachgebrauch untersuchen</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/ea95bb02-f116-42fb-941f-7af12e7f2050 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/ea95bb02-f116-42fb-941f-7af12e7f2050">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/c4e6462b-0905-4af1-bc19-c9f3be62e835"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.1.10</skos:notation>
        <skos:prefLabel xml:lang="de">kennen typische Formen digitaler Textstrukturierung (u. a. Hyperlinks, multimodale Textgestaltung) und nutzen sie zielorientiert (u. a. bei Recherchen und Präsentationen).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/ec20ea82-620a-4728-bbba-2384c849c045 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/ec20ea82-620a-4728-bbba-2384c849c045">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.3.2</skos:notation>
        <skos:prefLabel xml:lang="de">beachten gemeinsam entwickelte oder für öffentliche Kommunikation prototypische Gesprächsregeln; gestalten eine wertschätzende Gesprächsatmosphä¬re,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/efe25f2a-bc29-4b2b-97c5-e17a26b61026 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/efe25f2a-bc29-4b2b-97c5-e17a26b61026">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/db83727a-5a4b-438f-8bae-ec2e6166ab01"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>5.2.4.1</skos:notation>
        <skos:prefLabel xml:lang="de">nutzen die Konventionen der gesprochenen Standardsprache für die Gestaltung ihrer Äußerungen.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f07f0ba9-b5b1-4d78-a446-23375f96f398 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f07f0ba9-b5b1-4d78-a446-23375f96f398">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>4.4.7</skos:notation>
        <skos:prefLabel xml:lang="de">überprüfen eigene Annahmen zu Textaussagen und vorgegebene Deutungshypothesen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f0e7b014-6514-449b-b61f-b279e68bbf95 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f0e7b014-6514-449b-b61f-b279e68bbf95">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/7a928de1-6257-48ea-9e37-23e0752d8ba8"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">formulieren in vertrauten und unvertrauten Situationen angemessen; gehen auf die Gesprächspartnerin bzw. den Gesprächspartner und die jeweilige Situation ein,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f0f4294d-68d6-4f1c-937e-2bcbbe2fdf32 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f0f4294d-68d6-4f1c-937e-2bcbbe2fdf32">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/75cd0302-4b52-4cf3-8770-c84268188354"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>1.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">äußern sich verständlich und situationsadäquat auch in der gesprochenen Standardsprache,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/f1002003-0d2f-4a99-9814-3bdc4ab22a3f">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
