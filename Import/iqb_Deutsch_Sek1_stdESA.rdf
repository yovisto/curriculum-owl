        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/486af035-8c97-4b17-a658-c9c96711f626"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/660f9f0b-81aa-48cc-b81f-5f178eb6ac93"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/88113829-61bc-4e43-90ba-e40b3c8c8a70"/>
