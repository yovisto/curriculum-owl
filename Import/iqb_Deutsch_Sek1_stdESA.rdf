        <skos:notation>1.1.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">erzählen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8e58bbc2-c9dc-408a-b913-09f74c02b0bd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/8e58bbc2-c9dc-408a-b913-09f74c02b0bd">
