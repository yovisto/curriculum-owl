        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/"/>
        <skos:notation>2.4.7</skos:notation>
        <skos:prefLabel xml:lang="de">planen Texte kooperativ und strukturieren diese: z. B. Textbaupläne,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22des1/stdESA/8d07cde9-e806-410d-832e-792b1a55fe25 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22des1/stdESA/8d07cde9-e806-410d-832e-792b1a55fe25">
