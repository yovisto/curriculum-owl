    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/ce795aea-2592-4b5e-a041-d3c6d7a82da4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/fe94a6a2-c9c9-4faa-9d1e-e316def27092"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>2.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">messen Längen, Zeitspannen, Massen und Hohlmaße mit geeigneten Einheiten und unterschiedlichen Messgeräten sachgerecht,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/cf801bcd-a760-404e-840b-3f9eb5160c3b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/cf801bcd-a760-404e-840b-3f9eb5160c3b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e19264d5-ade4-4c20-9a93-937fcddf59c2"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>2.1.1</skos:notation>
        <skos:prefLabel xml:lang="de">vergleichen und ordnen Größen (Geldwerte, Längen, Zeitspannen, Massen, Flächeninhalte und Volumina),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/dcde6f10-8b62-49a3-ac4c-47eb510f8463 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/dcde6f10-8b62-49a3-ac4c-47eb510f8463">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/910a1f78-091e-4510-bbdf-71374b213502"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">erkennen, beschreiben und nutzen (z. B. zwei- und dreidimensionale Darstellungen zueinander in Beziehung setzen, wie Bauplan und Bauwerk, Körper und Netz) räumliche Beziehungen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/df2bc2cb-a531-4e3c-99bb-4fb40218bfc3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/df2bc2cb-a531-4e3c-99bb-4fb40218bfc3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/57b18910-51bc-4d3f-9d11-46a3d6ee5285"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>1.2.8</skos:notation>
        <skos:prefLabel xml:lang="de">kontrollieren Lösungen durch geeignete Vorgehensweisen (z. B. Überschlagsrechnung, Umkehroperation).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e19264d5-ade4-4c20-9a93-937fcddf59c2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e19264d5-ade4-4c20-9a93-937fcddf59c2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/33b83f7d-d316-4eaf-a345-c756cc08a924"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/cf801bcd-a760-404e-840b-3f9eb5160c3b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/16fd4e82-9534-4ab3-9351-363a252e0663"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/466c39cd-b4a9-4b84-b0d5-576c965f06a3"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/67bc3a29-0230-4f3f-a0d8-ef29c5665ca2"/>
        <skos:notation>2.1</skos:notation>
        <skos:prefLabel xml:lang="de">Über Größenvorstellungen verfügen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e37317dc-8999-4b3d-b3fd-9703c4511fdd"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e341194f-7715-4500-af91-bbf6334fec63"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e8b47e6b-6b61-4e5f-ab4e-19d6092a21f4"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/095ca3d2-35c8-441c-b220-4ee007fb412b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/2af36dea-2d3d-41b8-9b35-eead2b0d1e80"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/68fce65b-4595-43ee-87b0-21f433d6f2eb"/>
        <skos:notation>4.2</skos:notation>
        <skos:prefLabel xml:lang="de">Geometrische Figuren erkennen, benennen und darstellen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e341194f-7715-4500-af91-bbf6334fec63 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e341194f-7715-4500-af91-bbf6334fec63">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.2.1</skos:notation>
        <skos:prefLabel xml:lang="de">klassifizieren Körper und ebene Figuren nach Eigenschaften, ordnen Fachbegriffe zu und beschreiben Beziehungen zwischen geometrischen Figuren (z. B. Quadrat und Rechteck),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e36a53a0-b411-4a7d-b48f-60f6828b71a2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e36a53a0-b411-4a7d-b48f-60f6828b71a2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/c9955b6d-8383-4095-b5d1-c5b863c465cd"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>5.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">stellen Daten in Tabellen, Schaubildern und Diagrammen dar, auch unter Nutzung digitaler Werkzeuge, und entnehmen Informationen aus Tabellen, Schaubildern und Diagrammen,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e37317dc-8999-4b3d-b3fd-9703c4511fdd -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e37317dc-8999-4b3d-b3fd-9703c4511fdd">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:definition xml:lang="de">Diese Leitidee ist auf die Entwicklung des räumlichen Vorstellungsvermögens gerichtet und beinhaltet den Umgang mit Objekten in Ebene und Raum sowie darauf bezogene Prozesse wie das geometrische Abbilden. Konkrete Handlungserfahrungen werden vertieft, systematisch geordnet, genutzt und spiralcurricular erweitert. Übergreifend spielen dabei das Begriffsverständnis und das geometrische Zeichnen eine Rolle, indem Eigenschaften und Beziehungen in den Mittelpunkt rücken und geometrische Objekte mit geeigneten Medien (einschließlich digitaler Mathematikwerkzeuge) dargestellt werden.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/7c916dfd-b823-42f5-844b-24c37846e328"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/910a1f78-091e-4510-bbdf-71374b213502"/>
        <skos:notation>4</skos:notation>
        <skos:prefLabel xml:lang="de">Leitidee Raum und Form</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e4aec197-39b2-4242-a33b-f0e10c4ef0a2 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e4aec197-39b2-4242-a33b-f0e10c4ef0a2">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/5188bb26-d308-4592-bdc8-4872745231e3"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>3.1.3</skos:notation>
        <skos:prefLabel xml:lang="de">erkennen, stellen Gleichheit von mathematischen Ausdrücken dar und nutzen diese (z. B. Zahlen durch verschiedene Terme ausdrücken, Terme vergleichen).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/e8b47e6b-6b61-4e5f-ab4e-19d6092a21f4 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/e8b47e6b-6b61-4e5f-ab4e-19d6092a21f4">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">untersuchen und vergleichen ebene Figuren und Körper untersuchen und vergleichen (ebene Figuren auch hinsichtlich des Umfangs und Flächeninhalts, Körper auch hinsichtlich des Rauminhalts),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/ef897aca-3d68-4ca3-89ac-75b1f4776a30 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/ef897aca-3d68-4ca3-89ac-75b1f4776a30">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/015c95e2-4aed-4184-a641-13a892a4d5d9"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>1.1.2</skos:notation>
        <skos:prefLabel xml:lang="de">stellen Zahlen bis 1 000 000 auf verschiedene Weise dar (z. B. Anschauungsmittel, Stufenschrift, Stellenwerttabelle, Zifferndarstellung) und setzen diese zueinander in Beziehung,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/f3e6449f-de54-4c34-85a5-3fb7b9aac272 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/f3e6449f-de54-4c34-85a5-3fb7b9aac272">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/57b18910-51bc-4d3f-9d11-46a3d6ee5285"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>1.2.4</skos:notation>
        <skos:prefLabel xml:lang="de">verstehen mündliche und halbschriftliche Rechenstrategien zu den vier Grundrechenarten und setzen diese flexibel ein,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/f6182632-80e2-46cf-a837-e99d501f17a3 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/f6182632-80e2-46cf-a837-e99d501f17a3">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/c1750cc8-9247-4713-b2ca-7540699f4a09"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>5.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">schätzen Chancen für das Eintreten von Ereignissen bei alltäglichen Phänomenen oder einfachen Zufallsexperimenten ein und vergleichen (z. B. ist wahrscheinlicher als, hat größere Chancen als) diese datenbasiert.</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/fd3e0d10-6673-46ac-bf2a-ea589f91c484 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/fd3e0d10-6673-46ac-bf2a-ea589f91c484">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:definition xml:lang="de">Die Leitidee zielt in besonderer Weise auf die fachlich fundierte Erkundung von mathematischen Beziehungen und Gesetzmäßigkeiten zwischen Zahlen, Formen und Größen sowie deren Darstellungen und Eigenschaften. Ein Muster gleicht dabei eher einem Phänomen, in dem man eine Struktur – den Kern eines mathematischen Beziehungsgefüges – erkennen kann. Bei der Auseinandersetzung mit mathematischen Mustern und Darstellungen werden mathematisch relevante Strukturen (z. B. funktionale Beziehungen, Sortierungen, Ordnungen) erfasst und beschrieben, die dann wiederum in verschiedenen mathematischen Kontexten genutzt werden können.</skos:definition>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/263c0baa-8e08-4b95-81ab-98f68726ec5c"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/5188bb26-d308-4592-bdc8-4872745231e3"/>
        <skos:notation>3</skos:notation>
        <skos:prefLabel xml:lang="de">Leitidee Muster, Strukturen und funktionaler Zusammenhang</skos:prefLabel>
        <skos:topConceptOf rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/fe94a6a2-c9c9-4faa-9d1e-e316def27092 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/fe94a6a2-c9c9-4faa-9d1e-e316def27092">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/33b83f7d-d316-4eaf-a345-c756cc08a924"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/c2bff55c-ef27-495b-9ce7-21d00d56466b"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/cbeaca03-5899-4438-b451-2ab843879017"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/ce795aea-2592-4b5e-a041-d3c6d7a82da4"/>
        <skos:notation>2.2</skos:notation>
        <skos:prefLabel xml:lang="de">Größen messen und Maßangaben bestimmen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/015c95e2-4aed-4184-a641-13a892a4d5d9 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/015c95e2-4aed-4184-a641-13a892a4d5d9">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/89e5b347-60b3-4bfa-987b-025913ac43b5"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/ef897aca-3d68-4ca3-89ac-75b1f4776a30"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/19ab6818-944c-42f0-bf9d-16f8ebad9e53"/>
        <skos:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/3b9ea385-b376-4af6-aab6-fe0a34a8802d"/>
        <skos:notation>1.1</skos:notation>
        <skos:prefLabel xml:lang="de">Zahldarstellungen und Zahlbeziehungen verstehen</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/09fad8ff-7fdd-4703-9a42-4d4974d92df6 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/09fad8ff-7fdd-4703-9a42-4d4974d92df6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/7c916dfd-b823-42f5-844b-24c37846e328"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.3.1</skos:notation>
        <skos:prefLabel xml:lang="de">bilden ebene Figuren geometrisch ab (verkleinern, vergrößern und spiegeln),</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/095ca3d2-35c8-441c-b220-4ee007fb412b -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/095ca3d2-35c8-441c-b220-4ee007fb412b">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/e30bc8c9-def3-4da7-811b-282140662390"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.2.2</skos:notation>
        <skos:prefLabel xml:lang="de">erkennen Körper und ebene Figuren in der Umwelt wieder,</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/15ead2fa-5ab0-409d-a263-a5e4bdb3a086 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/15ead2fa-5ab0-409d-a263-a5e4bdb3a086">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/7c916dfd-b823-42f5-844b-24c37846e328"/>
        <skos:inScheme rdf:resource="https://w3id.org/iqb/bista22map/stdInhalt/"/>
        <skos:notation>4.3.3</skos:notation>
        <skos:prefLabel xml:lang="de">erkennen und beschreiben geometrische Abbildungen in der Umwelt oder in Mustern (z. B. in Bandornamenten).</skos:prefLabel>
    </NamedIndividual>
    


    <!-- https://w3id.org/iqb/bista22map/stdInhalt/16fd4e82-9534-4ab3-9351-363a252e0663 -->

    <NamedIndividual rdf:about="https://w3id.org/iqb/bista22map/stdInhalt/16fd4e82-9534-4ab3-9351-363a252e0663">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
