  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22dep/bistas/ec82b521-27a6-48bf-a0ba-400aa1ca5103">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <core:broader rdf:resource="https://w3id.org/iqb/bista22dep/bistas/addda665-85ff-4ba1-95ef-8b76fe872e46"/>
