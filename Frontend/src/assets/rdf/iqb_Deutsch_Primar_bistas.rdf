  <core:notation>1.1.2</core:notation>
  <core:prefLabel xml:lang="de">verfügen über einen angemessenen Wortschatz,</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

