  <rdf:type rdf:resource="https://dini-ag-kim.github.io/modell_lehrplaene#EducationalStandard"/>
  <core:notation>1.1.1</core:notation>
  <core:prefLabel xml:lang="de">äußern sich verständlich und situationsadäquat auch in der gesprochenen Standardsprache,</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

