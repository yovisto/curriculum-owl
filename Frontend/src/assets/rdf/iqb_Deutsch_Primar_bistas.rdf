  <core:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/5496e722-48b7-4833-99e4-2cc00ad9b846"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22dep/bistas/839dec82-bcaa-4a4e-bdf2-18aec42d707c"/>
  <mode:hasSubject rdf:resource="http://w3id.org/openeduhub/vocabs/discipline/120"/>
