  <rdf:type rdf:resource="https://dini-ag-kim.github.io/modell_lehrplaene#EducationalStandard"/>
  <core:definition xml:lang="de">Die Schülerinnen und Schüler lesen dem Lernstand entsprechende Texte sinnverstehend.</core:definition>
  <core:notation>3.3</core:notation>
  <core:prefLabel xml:lang="de">Lesefähigkeiten: Leseverstehen</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22dep/bistas/ac4f453f-aca9-48a6-b947-c45284651a6b">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
