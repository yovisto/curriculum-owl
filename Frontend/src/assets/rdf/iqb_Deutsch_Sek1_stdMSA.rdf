  <core:notation>5.2.5</core:notation>
  <core:prefLabel xml:lang="de">Ressourcen zur deutschen Sprache</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdMSA/9fd71bcf-0b51-4e2d-b6fe-858316b2a2fc">
