  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdMSA/1ba2f57f-18d8-4989-b27a-4674036e5920">
