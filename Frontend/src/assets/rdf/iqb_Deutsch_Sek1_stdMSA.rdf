  <core:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/b0e6363b-14e6-40a7-9d71-28705c9fb358"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/b8bc877f-1ecb-4c94-94ed-a0a2a625b6a6"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/2e59731c-bb73-4bdb-b32b-795d6f71afe6"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/4fd39d1f-9d48-4b30-80fc-619eb93dc14f"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/6f2857a7-3605-4961-af62-18f307ec6232"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA/7d18aef3-f2e5-419e-b513-d9ced8f70304"/>
  <core:topConceptOf rdf:resource="https://w3id.org/iqb/bista22des1/stdMSA"/>
