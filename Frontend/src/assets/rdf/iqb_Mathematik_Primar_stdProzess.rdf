  <core:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdProzess/2296f9b4-4aeb-42af-9fc5-8aea59e65382"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22map/stdProzess/32bb12b7-6d7a-41f2-b88a-9420750d7515"/>
  <core:topConceptOf rdf:resource="https://w3id.org/iqb/bista22map/stdProzess"/>
