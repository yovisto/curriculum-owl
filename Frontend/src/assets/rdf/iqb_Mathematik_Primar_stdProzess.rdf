  <core:notation>4.3</core:notation>
  <core:prefLabel xml:lang="de">formulieren zu Termen, Gleichungen und bildlichen Darstellungen Sachaufgaben.</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
