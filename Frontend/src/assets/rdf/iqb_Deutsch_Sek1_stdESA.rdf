  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdESA/de51f849-0e82-4002-871c-1dc9e81c5aab">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <core:broader rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/6017320d-36a0-4ef9-9648-e048ba82ffdf"/>
  <core:inScheme rdf:resource="https://w3id.org/iqb/bista22des1/stdESA"/>
  <mode:hasSubject rdf:resource="http://w3id.org/openeduhub/vocabs/discipline/120"/>
  <sche:educationalLevel rdf:resource="https://w3id.org/kim/educationalLevel/level_2"/>
