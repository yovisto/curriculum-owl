  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdESA/65511534-3b94-4a37-8931-0d6840f5989d">
