  <core:definition xml:lang="de">Die Schülerinnen und Schüler verfügen über Techniken zur Orientierung in und zwischen Texten.</core:definition>
  <core:notation>3.2</core:notation>
  <core:prefLabel xml:lang="de">Lesefertigkeiten: Über Lesetechniken verfügen</core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdESA/5fedea0e-1e34-4b19-820a-5a9e14344980">
