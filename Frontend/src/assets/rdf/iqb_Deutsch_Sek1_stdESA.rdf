  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/de51f849-0e82-4002-871c-1dc9e81c5aab"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/2d5e280d-6627-4036-909e-636a8515cf21"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/93637534-243a-42d3-9251-6d6ffd90a7f7"/>
