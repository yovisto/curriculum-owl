  <core:notation>2.2.1.2</core:notation>
  <core:prefLabel xml:lang="de">erschließen sich die Struktur von Kernwörtern, z. B. gliedern in Silben und Wortbestandteile, verlängern, flektieren, ableiten, erweitern, </core:prefLabel>
  <sche:yearBuilt rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</sche:yearBuilt>
</core:Concept>

<core:Concept rdf:about="https://w3id.org/iqb/bista22des1/stdESA/8197e8bf-54d8-4e84-bbd2-2e94e7ddc149">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
