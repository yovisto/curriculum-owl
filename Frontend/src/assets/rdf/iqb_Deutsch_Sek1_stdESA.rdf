  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/50b9a421-b5f5-4b6e-bff5-fe69cdc165fc"/>
  <core:narrower rdf:resource="https://w3id.org/iqb/bista22des1/stdESA/574a72fe-53f3-47bc-8787-771c3e980def"/>
